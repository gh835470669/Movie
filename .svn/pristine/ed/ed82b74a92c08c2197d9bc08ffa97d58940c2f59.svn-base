INSERT INTO `game`(title, create_time, company, preinfo, preimage, type, system, display_device, control_device, download_url, download_num, installation_size, score, source, content) VALUES ('横冲直撞','2015-12-8 00:00:00',360,'横冲直撞是一款第一人称的VR FPS游戏，画面风格走卡通渲染风。而游戏的界面让你看起来有一种漫画版GTA的感觉，只不过并没有抢车这一个选项。游戏中你的目标只有一个，那就是干掉所有的敌人！<br /><br />游戏解压后会有标识6.0驱动游戏版本和7.0驱动游戏版本，不过就小编测试结果来看，5.0驱动也是可以运行的，如果运行出现问题，那么请更换驱动。<br /><br />适用于：Deepoon大朋头显、Oculus DK2以及可以需要安装oculus驱动的其他头显','./game/preImages/c/20160806132507184.jpg','格斗游戏',2,'Oculus DK2|Oculus DK1|3Glasses|DeePoon','手柄|键鼠','http://pcdl.913vr.com/pc/hengchongzhizhuang.zip',10,'105MB',10.0,'913vr','<p>横冲直撞是一款第一人称的VR FPS游戏，画面风格走卡通渲染风。而游戏的界面让你看起来有一种漫画版GTA的感觉，只不过并没有抢车这一个选项。游戏中你的目标只有一个，那就是干掉所有的敌人！<br /><br />游戏解压后会有标识6.0驱动游戏版本和7.0驱动游戏版本，不过就小编测试结果来看，5.0驱动也是可以运行的，如果运行出现问题，那么请更换驱动。<br /><br />适用于：Deepoon大朋头显、Oculus DK2以及可以需要安装oculus驱动的其他头显</p><p><img src="./game/details/c/20160806132507185.jpg"/></p><p><img src="./game/details/c/20160806132507186.jpg"/></p><p><img src="./game/details/c/20160806132507187.jpg"/></p>');
INSERT INTO `game`(title, create_time, company, preinfo, preimage, type, system, display_device, control_device, download_url, download_num, installation_size, score, source, content) VALUES ('Lilith-M','2015-12-10 00:00:00',394,'《Lilith-M》是一款沉浸式VR解谜过关游戏。该游戏以小女孩莉莉丝寻找记忆解开实验室真相为线索划分了7个章节。 《Lilith-M 先行版》为精简的前五章游戏内容，主要包括双人过关与单人大关卡过关两种形式。&nbsp;','./game/preImages/c/20160806132507188.jpg','角色扮演',1,'Oculus DK2|Oculus DK1|DeePoon','手柄|键鼠','http://pcdl.913vr.com/pc/Lilith-M.zip',10,'172MB',4.0,'913vr','<p>《Lilith-M》是一款沉浸式VR解谜过关游戏。该游戏以小女孩莉莉丝寻找记忆解开实验室真相为线索划分了7个章节。 《Lilith-M 先行版》为精简的前五章游戏内容，主要包括双人过关与单人大关卡过关两种形式。&nbsp;</p><p><img src="./game/details/c/20160806132507189.jpg"/></p><p><img src="./game/details/c/20160806132507190.jpg"/></p><p><img src="./game/details/c/20160806132507191.jpg"/></p>');
INSERT INTO `game`(title, create_time, company, preinfo, preimage, type, system, display_device, control_device, download_url, download_num, installation_size, score, source, content) VALUES ('滑雪车','2015-12-11 00:00:00',360,'这是一个很棒的游戏，玩家可以坐在雪车上在雪中赛道感受极速的感觉，坐上去，操作很简单只要控制左右方向就可以啦，追求速度的童鞋们速来感受...','./game/preImages/c/20160806132507192.jpg','动作游戏',1,'Oculus DK1|3Glasses','键鼠','http://pcdl.913vr.com/pc/huaxueche.zip',10,'29.7MB',4.0,'913vr','<p>这是一个很棒的游戏，玩家可以坐在雪车上在雪中赛道感受极速的感觉，坐上去，操作很简单只要控制左右方向就可以啦，追求速度的童鞋们速来感受...</p><p><img src="./game/details/c/20160806132507193.jpg"/></p><p><img src="./game/details/c/20160806132507194.jpg"/></p><p><img src="./game/details/c/20160806132507195.jpg"/></p>');
INSERT INTO `game`(title, create_time, company, preinfo, preimage, type, system, display_device, control_device, download_url, download_num, installation_size, score, source, content) VALUES ('太平洋风云','2015-12-10 00:00:00',387,'《太平洋风云》是蓝鳍鱼开发的一部二战题材的第一人称战争游戏。该游戏第一章分为空战、巷战，夺岛战，夜袭等多种场景。《太平洋风云demo》演示版本主要是《轰炸龙骧号》关卡的空战部分。','./game/preImages/c/20160806132508196.jpg','格斗游戏',1,'Oculus DK2|Oculus DK1|3Glasses|DeePoon','手柄|键鼠','http://pcdl.913vr.com/pc/taipingyangfengyun.zip',10,'139MB',3.0,'913vr','<p>《太平洋风云》是蓝鳍鱼开发的一部二战题材的第一人称战争游戏。该游戏第一章分为空战、巷战，夺岛战，夜袭等多种场景。《太平洋风云demo》演示版本主要是《轰炸龙骧号》关卡的空战部分。</p><p><img src="./game/details/c/20160806132508197.png"/></p><p><img src="./game/details/c/20160806132508198.jpg"/></p><p><img src="./game/details/c/20160806132508199.jpg"/></p>');
