INSERT INTO `game`(title, create_time, company, preinfo, preimage, type, system, display_device, control_device, download_url, download_num, installation_size, score, source, content) VALUES ('赛艇会','2016-07-11 00:00:00',298,'在水上操纵帆船，利用风向做出各种转向和加速的动作，你会发现，这个游戏还是挺有趣的。','./game/preImages/b/2016080517004472.jpg','休闲益智',1,'HTC Vive','手柄|键鼠','http://resource.vr-store.cn/appfile/55291a8920574343b7d9407790db03fc.zip',10,'281.1M',10.0,'591vr','<p>在水上操纵帆船，利用风向做出各种转向和加速的动作，你会发现，这个游戏还是挺有趣的。</p><p><img src="./game/details/b/2016080517004473.jpg"></p><p><img src="./game/details/b/2016080517004474.jpg"></p><p><img src="./game/details/b/2016080517004475.jpg"></p>');
INSERT INTO `game`(title, create_time, company, preinfo, preimage, type, system, display_device, control_device, download_url, download_num, installation_size, score, source, content) VALUES ('VR俄罗斯方块','2016-07-21 00:00:00',611,'《VR俄罗斯方块》是一款虚拟现实休闲益智游戏。相信很多玩家朋友们都玩过俄罗斯方块，不管是小时候的小霸王插卡游戏机还是安装5号电池的黑白机，俄罗斯方块一定是一款百玩不腻的经典游戏，而今天为大家打来的就是这么一块以俄罗斯方块为基础开发的VR版俄罗斯方块游戏，可以更好的让玩家沉浸在游戏之中。','./game/preImages/b/2016080517004476.jpg','休闲益智',1,'Oculus DK2','','http://resource.vr-store.cn/appfile/f411437e75c640f3b2dd0913e5e49d1b.zip',10,'41.77M',10.0,'591vr','<p>《VR俄罗斯方块》是一款虚拟现实休闲益智游戏。相信很多玩家朋友们都玩过俄罗斯方块，不管是小时候的小霸王插卡游戏机还是安装5号电池的黑白机，俄罗斯方块一定是一款百玩不腻的经典游戏，而今天为大家打来的就是这么一块以俄罗斯方块为基础开发的VR版俄罗斯方块游戏，可以更好的让玩家沉浸在游戏之中。</p><p><img src="./game/details/b/2016080517004477.jpg"></p><p><img src="./game/details/b/2016080517004478.jpg"></p><p><img src="./game/details/b/2016080517004479.jpg"></p>');
INSERT INTO `game`(title, create_time, company, preinfo, preimage, type, system, display_device, control_device, download_url, download_num, installation_size, score, source, content) VALUES ('梦点·危机','2016-07-11 00:00:00',299,'《梦点·危机》需要玩家穿戴htcVIVE硬件设备来进行游戏体验。游戏的剧情起始于异世界。玩家扮演一个拥有强大精神能力的人，被“星灵”召唤到了传送门前帮助他们寻找宇宙能量核心，解决世界危机，从而踏上了冒险征途。','./game/preImages/b/2016080517004480.jpg','冒险解谜',1,'HTC Vive','手柄|键鼠','http://resource.vr-store.cn/appfile/0c396ee093244ce9b17a7380da89b26d.zip',10,'504.99M',10.0,'591vr','<p>《梦点·危机》需要玩家穿戴htcVIVE硬件设备来进行游戏体验。游戏的剧情起始于异世界。玩家扮演一个拥有强大精神能力的人，被“星灵”召唤到了传送门前帮助他们寻找宇宙能量核心，解决世界危机，从而踏上了冒险征途。</p><p><img src="./game/details/b/2016080517004481.jpg"></p><p><img src="./game/details/b/2016080517004482.jpg"></p><p><img src="./game/details/b/2016080517004483.jpg"></p>');
INSERT INTO `game`(title, create_time, company, preinfo, preimage, type, system, display_device, control_device, download_url, download_num, installation_size, score, source, content) VALUES ('未来的幽灵','2016-06-27 00:00:00',300,'未来的幽灵（Ghosts of our Future）是一款场景体验类型的游戏，游戏共有6个场景，讲述的是关于人类对资源的胃口破坏了地球生态系统，从而演变为环境与人类之间的战争。该游戏不需要任何操作，只需玩家静静体验即可。','./game/preImages/b/2016080517004484.jpg','冒险解谜',1,'Oculus DK1|Oculus DK2|Deepoon','键鼠','http://resource.vr-store.cn/appfile/db8232f5553a42e19dc16c3103aa0ab5.zip',10,'1.19G',10.0,'591vr','<p>未来的幽灵（Ghosts of our Future）是一款场景体验类型的游戏，游戏共有6个场景，讲述的是关于人类对资源的胃口破坏了地球生态系统，从而演变为环境与人类之间的战争。该游戏不需要任何操作，只需玩家静静体验即可。</p><p><img src="./game/details/b/2016080517004485.jpg"></p><p><img src="./game/details/b/2016080517004486.jpg"></p><p><img src="./game/details/b/2016080517004487.jpg"></p>');
INSERT INTO `game`(title, create_time, company, preinfo, preimage, type, system, display_device, control_device, download_url, download_num, installation_size, score, source, content) VALUES ('星球大战：塔图因试练','2016-07-19 00:00:00',13,'《 Star Wars: Trials on Tatooine》（《星球大战：塔图因试炼》）是由ILMxLAB工作室发布的一款简短的VR游戏。游戏支持目前支持HTC vive头显，游戏为第一视角游戏，与星球大战系列电影里的场景一样，游戏主人公将会被安排成为绝地武士组织的一员。尽管整个游戏只持续几分钟时间，但是就在这短短的几分钟时间里，你将帮助汉·索罗（Han Solo）修复千年隼号（宇宙飞船），与R2-D2（机器人）见面，甚至你还会拿起光剑，去对抗围绕着你的众多帝国突击队军团。玩家将有史以来第一次通过操纵Vive的运动控制器来使用从未使用过的“光剑”。','./game/preImages/b/2016080517004588.jpg','漫游体验',1,'HTC Vive','手柄','http://resource.vr-store.cn/appfile/TrialsOnTatooine.zip',10,'2.41G',10.0,'591vr','<p>《 Star Wars: Trials on Tatooine》（《星球大战：塔图因试炼》）是由ILMxLAB工作室发布的一款简短的VR游戏。游戏支持目前支持HTC vive头显，游戏为第一视角游戏，与星球大战系列电影里的场景一样，游戏主人公将会被安排成为绝地武士组织的一员。尽管整个游戏只持续几分钟时间，但是就在这短短的几分钟时间里，你将帮助汉·索罗（Han Solo）修复千年隼号（宇宙飞船），与R2-D2（机器人）见面，甚至你还会拿起光剑，去对抗围绕着你的众多帝国突击队军团。玩家将有史以来第一次通过操纵Vive的运动控制器来使用从未使用过的“光剑”。</p><p><img src="./game/details/b/2016080517004589.jpg"></p><p><img src="./game/details/b/2016080517004590.jpg"></p><p><img src="./game/details/b/2016080517004591.jpg"></p>');
INSERT INTO `game`(title, create_time, company, preinfo, preimage, type, system, display_device, control_device, download_url, download_num, installation_size, score, source, content) VALUES ('深海潜水体验','2016-06-27 00:00:00',301,'这款是 GE公司致力于研究未来开采石油和天然气的“海底工厂”，然后委托Kite & Lightning 团队来开发的一款海底潜水的demo！我们生活的地球表面70%都是海洋的世界，其中绝大多数都没有被开发。所以这款游戏就是玩家坐在探索潜艇里进入深海探险，画质优质，场景逼真是这款游戏的特色。','./game/preImages/b/2016080517004592.jpg','冒险解谜',1,'Oculus DK1|Oculus DK2|Deepoon','键鼠','http://resource.vr-store.cn/appfile/Subsea_Experience-pc.zip',10,'1.72G',10.0,'591vr','<p>这款是 GE公司致力于研究未来开采石油和天然气的“海底工厂”，然后委托Kite & Lightning 团队来开发的一款海底潜水的demo！我们生活的地球表面70%都是海洋的世界，其中绝大多数都没有被开发。所以这款游戏就是玩家坐在探索潜艇里进入深海探险，画质优质，场景逼真是这款游戏的特色。</p><p><img src="./game/details/b/2016080517004593.jpg"></p><p><img src="./game/details/b/2016080517004594.jpg"></p><p><img src="./game/details/b/2016080517004595.jpg"></p>');
INSERT INTO `game`(title, create_time, company, preinfo, preimage, type, system, display_device, control_device, download_url, download_num, installation_size, score, source, content) VALUES ('天文台大探险','2016-05-25 00:00:00',302,'不同风格的游戏集合在一起，玩家可以在这里体验到宇宙探险《千变幻境（Phantasmagoria）》、极限下滑体验《Hamster Slide》、恐怖挑战《抗体（Antibody）》、益智猜谜《岛屿（The Island）》等多种类别的游戏场景。','./game/preImages/b/2016080517004696.jpg','冒险解谜',1,'Oculus DK1|Oculus DK2|Deepoon|HTC Vive','手柄|键鼠','http://resource.vr-store.cn/appfile/19763a16a9644b6799e03699294e194f.zip',10,'849.51M',10.0,'591vr','<p>不同风格的游戏集合在一起，玩家可以在这里体验到宇宙探险《千变幻境（Phantasmagoria）》、极限下滑体验《Hamster Slide》、恐怖挑战《抗体（Antibody）》、益智猜谜《岛屿（The Island）》等多种类别的游戏场景。</p><p><img src="./game/details/b/2016080517004697.jpg"></p><p><img src="./game/details/b/2016080517004698.jpg"></p><p><img src="./game/details/b/2016080517004699.jpg"></p>');
INSERT INTO `game`(title, create_time, company, preinfo, preimage, type, system, display_device, control_device, download_url, download_num, installation_size, score, source, content) VALUES ('巫师华尔兹','2016-06-21 00:00:00',303,'魔法需要玩家自己合成，将材料、药水扔进锅里，合成出来的魔法都会出现在你的手里，快来尝试下吧！','./game/preImages/b/20160805170046100.jpg','休闲益智',1,'HTC Vive','手柄|键鼠','http://resource.vr-store.cn/appfile/d3954acb9227495cba7f19dc8cf6d90c.zip',10,'364.32M',10.0,'591vr','<p>魔法需要玩家自己合成，将材料、药水扔进锅里，合成出来的魔法都会出现在你的手里，快来尝试下吧！</p><p><img src="./game/details/b/20160805170046101.jpg"></p><p><img src="./game/details/b/20160805170046102.jpg"></p><p><img src="./game/details/b/20160805170046103.jpg"></p>');
INSERT INTO `game`(title, create_time, company, preinfo, preimage, type, system, display_device, control_device, download_url, download_num, installation_size, score, source, content) VALUES ('太空炮弹','2016-05-06 00:00:00',304,'《银河碰碰球》的升级版本游戏！玩家扮演一个小红球，你很爱钻石，想必你已经知道该怎么玩这个游戏了！收集所有的钻石可以进入下一个游戏场景；看到有diamont前往别错过，收集它你可以获得抽金球的机会；小心，不能让黑球触屏到你，否则它会偷走你所有的东西；记住，你只是一个小球，你不能飞，所以前往小心，别掉下去！','./game/preImages/b/20160805170046104.jpg','休闲益智',1,'Oculus DK2|Deepoon|Oculus CV1','手柄','http://resource.vr-store.cn/appfile/d4f5de52dea34956a3888ba697c9858f.zip',10,'87.93M',10.0,'591vr','<p>《银河碰碰球》的升级版本游戏！玩家扮演一个小红球，你很爱钻石，想必你已经知道该怎么玩这个游戏了！收集所有的钻石可以进入下一个游戏场景；看到有diamont前往别错过，收集它你可以获得抽金球的机会；小心，不能让黑球触屏到你，否则它会偷走你所有的东西；记住，你只是一个小球，你不能飞，所以前往小心，别掉下去！</p><p><img src="./game/details/b/20160805170046105.jpg"></p><p><img src="./game/details/b/20160805170046106.jpg"></p><p><img src="./game/details/b/20160805170046107.jpg"></p>');
INSERT INTO `game`(title, create_time, company, preinfo, preimage, type, system, display_device, control_device, download_url, download_num, installation_size, score, source, content) VALUES ('涂鸦实验室','2016-06-22 00:00:00',305,'一款可以让你尽情涂鸦的软件，尝试着去创造属于你的作品！','./game/preImages/b/20160805170047108.jpg','休闲益智',1,'HTC Vive','手柄|键鼠','http://resource.vr-store.cn/appfile/7c63bf946f624011ac4ce075acd61499.zip',10,'26.44M',10.0,'591vr','<p>一款可以让你尽情涂鸦的软件，尝试着去创造属于你的作品！</p><p><img src="./game/details/b/20160805170047109.jpg"></p><p><img src="./game/details/b/20160805170047110.jpg"></p><p><img src="./game/details/b/20160805170047111.jpg"></p>');
INSERT INTO `game`(title, create_time, company, preinfo, preimage, type, system, display_device, control_device, download_url, download_num, installation_size, score, source, content) VALUES ('星界圣主','2016-05-10 00:00:00',306,'玩家被迫降落在一个神秘的星球上，这里曾经是繁华的古老外星帝国，但如今是萧条一片，而玩家必须在这里寻找到能源基地，为你的飞船提供足够的能量，才能顺利离开这里。此版本属于试玩版本，想要体验正式版本的需要购买！','./game/preImages/b/20160805170047112.jpg','第一人称射击',1,'HTC Vive','手柄|键鼠','http://resource.vr-store.cn/appfile/starsinmaster.zip',10,'2.47G',10.0,'591vr','<p>玩家被迫降落在一个神秘的星球上，这里曾经是繁华的古老外星帝国，但如今是萧条一片，而玩家必须在这里寻找到能源基地，为你的飞船提供足够的能量，才能顺利离开这里。此版本属于试玩版本，想要体验正式版本的需要购买！</p><p><img src="./game/details/b/20160805170047113.png"></p><p><img src="./game/details/b/20160805170047114.png"></p><p><img src="./game/details/b/20160805170047115.jpg"></p>');
INSERT INTO `game`(title, create_time, company, preinfo, preimage, type, system, display_device, control_device, download_url, download_num, installation_size, score, source, content) VALUES ('迪士尼电影','2016-05-26 00:00:00',307,'足不出户也可以游玩迪士尼乐园，在这里，你可以体验漫威、卢卡斯等主题电影世界，还可以和自己喜欢的角色互动，包括《丛林之书》、《美国队长》、《星球大战》等。','./game/preImages/b/20160805170047116.jpg','漫游体验',1,'Oculus DK1|Oculus DK2|HTC Vive','手柄|键鼠','http://resource.vr-store.cn/appfile/1f198206f7bd4a3e8f04343dbe3add44.zip',10,'880.94M',10.0,'591vr','<p>足不出户也可以游玩迪士尼乐园，在这里，你可以体验漫威、卢卡斯等主题电影世界，还可以和自己喜欢的角色互动，包括《丛林之书》、《美国队长》、《星球大战》等。</p><p><img src="./game/details/b/20160805170047117.jpg"></p><p><img src="./game/details/b/20160805170047118.jpg"></p><p><img src="./game/details/b/20160805170047119.jpg"></p>');
INSERT INTO `game`(title, create_time, company, preinfo, preimage, type, system, display_device, control_device, download_url, download_num, installation_size, score, source, content) VALUES ('管道过山车','2016-06-23 00:00:00',611,'这是一款过山车题材的游戏，坐在这款过山车里感受风驰电掣、有惊无险的快感令不少人着迷，你也想体验一把这种快感吗？游戏并不需要什么太难的操控，只需要通过手柄的左摇杆键控制前后左右即可,根据前后左右，还可以调节音乐的节奏！','./game/preImages/b/20160805170047120.jpg','冒险解谜',1,'Oculus DK1|Oculus DK2|Deepoon','手柄|键鼠','http://resource.vr-store.cn/appfile/4e8e4d57a6164acdb2f7b44c470a76fa.zip',10,'53.43M',10.0,'591vr','<p>这是一款过山车题材的游戏，坐在这款过山车里感受风驰电掣、有惊无险的快感令不少人着迷，你也想体验一把这种快感吗？游戏并不需要什么太难的操控，只需要通过手柄的左摇杆键控制前后左右即可,根据前后左右，还可以调节音乐的节奏！</p><p><img src="./game/details/b/20160805170047121.jpg"></p><p><img src="./game/details/b/20160805170047122.jpg"></p><p><img src="./game/details/b/20160805170047123.jpg"></p>');
INSERT INTO `game`(title, create_time, company, preinfo, preimage, type, system, display_device, control_device, download_url, download_num, installation_size, score, source, content) VALUES ('涌动的方块二','2016-05-27 00:00:00',308,'这是一个场景的体验游戏，里面有跳动的方块会组成的不同的形状，还有动感的音乐，跳舞的方块，让你置身一个超动感的音乐世界。欣赏眼前随节奏变化的场景，相信你的身体也会跟随节奏一起high动。','./game/preImages/b/20160805170047124.jpg','漫游体验',1,'Oculus DK1|Oculus DK2|Deepoon|HTC Vive','手柄|键鼠','http://resource.vr-store.cn/appfile/7808a805f1fb4d9f88a107c0f14e36f5.zip',10,'232.38M',10.0,'591vr','<p>这是一个场景的体验游戏，里面有跳动的方块会组成的不同的形状，还有动感的音乐，跳舞的方块，让你置身一个超动感的音乐世界。欣赏眼前随节奏变化的场景，相信你的身体也会跟随节奏一起high动。</p><p><img src="./game/details/b/20160805170047125.jpg"></p><p><img src="./game/details/b/20160805170047126.jpg"></p><p><img src="./game/details/b/20160805170047127.jpg"></p>');
INSERT INTO `game`(title, create_time, company, preinfo, preimage, type, system, display_device, control_device, download_url, download_num, installation_size, score, source, content) VALUES ('涂鸦','2016-05-06 00:00:00',305,'使用你的HTC Vive手柄操控画笔制作3D作品，创造任何你喜欢的事物，比如恐龙、城堡、蜘蛛侠等等。同时你还可以和你的作品进行互动，玩弄、丢弃它们，只要你喜欢，没有什么是不可以的！','./game/preImages/b/20160805170048128.jpg','漫游体验',1,'HTC Vive','手柄|键鼠','http://resource.vr-store.cn/appfile/012863aac1704cbbb1d253d6efd4b62a.zip',10,'26.44M',10.0,'591vr','<p>使用你的HTC Vive手柄操控画笔制作3D作品，创造任何你喜欢的事物，比如恐龙、城堡、蜘蛛侠等等。同时你还可以和你的作品进行互动，玩弄、丢弃它们，只要你喜欢，没有什么是不可以的！</p><p><img src="./game/details/b/20160805170048129.jpg"></p><p><img src="./game/details/b/20160805170048130.png"></p><p><img src="./game/details/b/20160805170048131.jpg"></p>');
INSERT INTO `game`(title, create_time, company, preinfo, preimage, type, system, display_device, control_device, download_url, download_num, installation_size, score, source, content) VALUES ('深海运动','2016-05-27 00:00:00',309,'使用HTC在海底打保龄球会是怎么样的一种体验？国外开发商Jumping Llamas 就为我们提供了这样一个平台，感受水下运动的别样风趣。目前这款试玩游戏只有一款保龄球运动，接下来陆续还会加入棒球、射箭、飞盘等运动。开发商Jumping Llamas表示，将来还会带来多人在线的形式，让更多的人来体验VR比赛的刺激。','./game/preImages/b/20160805170048132.jpg','动作游戏',1,'HTC Vive','手柄|键鼠','http://resource.vr-store.cn/appfile/66275dbbc8bf44bfac42cb36322fe98d.zip',10,'285.14M',10.0,'591vr','<p>使用HTC在海底打保龄球会是怎么样的一种体验？国外开发商Jumping Llamas 就为我们提供了这样一个平台，感受水下运动的别样风趣。目前这款试玩游戏只有一款保龄球运动，接下来陆续还会加入棒球、射箭、飞盘等运动。开发商Jumping Llamas表示，将来还会带来多人在线的形式，让更多的人来体验VR比赛的刺激。</p><p><img src="./game/details/b/20160805170048133.jpg"></p><p><img src="./game/details/b/20160805170048134.jpg"></p><p><img src="./game/details/b/20160805170048135.jpg"></p>');
INSERT INTO `game`(title, create_time, company, preinfo, preimage, type, system, display_device, control_device, download_url, download_num, installation_size, score, source, content) VALUES ('夜间咖啡馆','2016-06-21 00:00:00',310,'以《夜间咖啡馆》为中心，将梵高的几幅作品嵌入场景，呈现给体验者。通过vr让体验者能够以第一视角去发现梵高的世界。','./game/preImages/b/20160805170048136.jpg','休闲益智',1,'Oculus DK1|Oculus DK2|Deepoon|HTC Vive','手柄|键鼠','http://resource.vr-store.cn/appfile/6bccc60ec90e4901974fa93b48c0e72f.zip',10,'115.54M',10.0,'591vr','<p>以《夜间咖啡馆》为中心，将梵高的几幅作品嵌入场景，呈现给体验者。通过vr让体验者能够以第一视角去发现梵高的世界。</p><p><img src="./game/details/b/20160805170048137.jpg"></p><p><img src="./game/details/b/20160805170048138.jpg"></p><p><img src="./game/details/b/20160805170048139.jpg"></p>');
INSERT INTO `game`(title, create_time, company, preinfo, preimage, type, system, display_device, control_device, download_url, download_num, installation_size, score, source, content) VALUES ('玫瑰与我','2016-06-21 00:00:00',311,'这是一部身临其境的虚拟现实动画电影。是由Eugene Chung ，Jimmy Maidens和Terry Kaleas开发商开发完成的VR短片，短片内容是根据法国作家安东尼·德·圣.埃克苏佩里于1942年写成的著名儿童文学短篇小说《小王子》改编而成。目前这款游戏支持HTC vive，喜欢小王子的玩家不要错过这款游戏哟！','./game/preImages/b/20160805170048140.jpg','休闲益智',1,'HTC Vive','手柄|键鼠','http://resource.vr-store.cn/appfile/8ad898191cf34459a9d03b1bfe53925f.rar',10,'533.38M',10.0,'591vr','<p>这是一部身临其境的虚拟现实动画电影。是由Eugene Chung ，Jimmy Maidens和Terry Kaleas开发商开发完成的VR短片，短片内容是根据法国作家安东尼·德·圣.埃克苏佩里于1942年写成的著名儿童文学短篇小说《小王子》改编而成。目前这款游戏支持HTC vive，喜欢小王子的玩家不要错过这款游戏哟！</p><p><img src="./game/details/b/20160805170048141.jpg"></p><p><img src="./game/details/b/20160805170048142.jpg"></p><p><img src="./game/details/b/20160805170048143.jpg"></p>');
INSERT INTO `game`(title, create_time, company, preinfo, preimage, type, system, display_device, control_device, download_url, download_num, installation_size, score, source, content) VALUES ('无理繁荣：序言','2016-06-22 00:00:00',312,'探索太空的一个免费游戏，游戏本身需求2mx2m的房间模式，但对站立模式支持也还不错。利用手势交互来改变周围的环境。Irrational Exuberance: Prologue，给人一种破坏的快感。当然，视觉方面的冲击也是必不可少的。','./game/preImages/b/20160805170049144.jpg','冒险解谜',1,'HTC Vive','手柄|键鼠','http://resource.vr-store.cn/appfile/4389234b4a3041e8b7a7e7616a0854fd.zip',10,'99.36M',10.0,'591vr','<p>探索太空的一个免费游戏，游戏本身需求2mx2m的房间模式，但对站立模式支持也还不错。利用手势交互来改变周围的环境。Irrational Exuberance: Prologue，给人一种破坏的快感。当然，视觉方面的冲击也是必不可少的。</p><p><img src="./game/details/b/20160805170049145.jpg"></p><p><img src="./game/details/b/20160805170049146.jpg"></p><p><img src="./game/details/b/20160805170049147.jpg"></p>');
INSERT INTO `game`(title, create_time, company, preinfo, preimage, type, system, display_device, control_device, download_url, download_num, installation_size, score, source, content) VALUES ('捍卫基地','2016-05-24 00:00:00',313,'这是一个免费的FPS科幻游戏。目前，开放体验的游戏场景有3个级别，每个级别都有大量的怪兽攻击你。玩家拥有强有力的炮火，是否能够阻止怪兽来袭？考验玩家的爆发力时候到了！目前这款游戏还属于测试阶段，正式版本的游戏即将推出，喜欢的玩家拭目以待！','./game/preImages/b/20160805170049148.jpg','第一人称射击',1,'Oculus DK2|Deepoon','手柄|键鼠','http://resource.vr-store.cn/appfile/bc211635299d483eb1c427a6527f146e.zip',10,'319.89M',10.0,'591vr','<p>这是一个免费的FPS科幻游戏。目前，开放体验的游戏场景有3个级别，每个级别都有大量的怪兽攻击你。玩家拥有强有力的炮火，是否能够阻止怪兽来袭？考验玩家的爆发力时候到了！目前这款游戏还属于测试阶段，正式版本的游戏即将推出，喜欢的玩家拭目以待！</p><p><img src="./game/details/b/20160805170049149.png"></p><p><img src="./game/details/b/20160805170049150.png"></p><p><img src="./game/details/b/20160805170049151.png"></p>');
INSERT INTO `game`(title, create_time, company, preinfo, preimage, type, system, display_device, control_device, download_url, download_num, installation_size, score, source, content) VALUES ('飞碟射击','2016-06-21 00:00:00',314,'第一人称射击游戏，拥有Skeet Mode，Trap Mode和Quickdraw Mode，每个模式都能让你拥有射击的乐趣，尝试下击中飞行中的东西吧！','./game/preImages/b/20160805170049152.jpg','动作游戏',1,'HTC Vive','手柄|键鼠','http://resource.vr-store.cn/appfile/e8a57bef704e44ebbf333a7a7aea94bc.zip',10,'174.11M',10.0,'591vr','<p>第一人称射击游戏，拥有Skeet Mode，Trap Mode和Quickdraw Mode，每个模式都能让你拥有射击的乐趣，尝试下击中飞行中的东西吧！</p><p><img src="./game/details/b/20160805170049153.jpg"></p><p><img src="./game/details/b/20160805170049154.jpg"></p><p><img src="./game/details/b/20160805170049155.jpg"></p>');
INSERT INTO `game`(title, create_time, company, preinfo, preimage, type, system, display_device, control_device, download_url, download_num, installation_size, score, source, content) VALUES ('棒球击打','2016-06-22 00:00:00',315,'PITCH-HIT是一款免费的VR棒球体验游戏。进入游戏后，你只需要做尽情的挥击，将场地内的瓶子、盘子、玻璃、雕塑等各种物品给破毁掉，玩起来十分的欢乐。','./game/preImages/b/20160805170049156.jpg','休闲益智',1,'HTC Vive','手柄|键鼠','http://resource.vr-store.cn/appfile/3e543202d7c74304a6a39831800d4854.zip',10,'319.38M',10.0,'591vr','<p>PITCH-HIT是一款免费的VR棒球体验游戏。进入游戏后，你只需要做尽情的挥击，将场地内的瓶子、盘子、玻璃、雕塑等各种物品给破毁掉，玩起来十分的欢乐。</p><p><img src="./game/details/b/20160805170049157.jpg"></p><p><img src="./game/details/b/20160805170049158.jpg"></p><p><img src="./game/details/b/20160805170049159.jpg"></p>');
INSERT INTO `game`(title, create_time, company, preinfo, preimage, type, system, display_device, control_device, download_url, download_num, installation_size, score, source, content) VALUES ('未变异者','2016-06-22 00:00:00',316,'Unturned出了VR版，在使用HTCvive时，需要有至少 2 米 x 1.5 米的游玩范围，这样你就可以进入游戏中跟人物进行互动。','./game/preImages/b/20160805170050160.jpg','冒险解谜',1,'HTC Vive','手柄|键鼠','http://resource.vr-store.cn/appfile/0766e0b4b6e3426a8298c9d0684dcb1e.zip',10,'228.83M',10.0,'591vr','<p>Unturned出了VR版，在使用HTCvive时，需要有至少 2 米 x 1.5 米的游玩范围，这样你就可以进入游戏中跟人物进行互动。</p><p><img src="./game/details/b/20160805170050161.jpg"></p><p><img src="./game/details/b/20160805170050162.jpg"></p><p><img src="./game/details/b/20160805170050163.jpg"></p>');
INSERT INTO `game`(title, create_time, company, preinfo, preimage, type, system, display_device, control_device, download_url, download_num, installation_size, score, source, content) VALUES ('歌罗西二','2016-05-06 00:00:00',317,'新版《歌罗西》兼容Oculus Rift消费版头显和HTC Vive场景以观众为主导的故事叙述，故事围绕遗失了很久的伟大人物展开，它探索了恐惧、权利和尊重等主题。这些人物是以设计复杂的巨人形象出现，场景中没有人物对话，只有一个老人的独白叙述着整个故事。','./game/preImages/b/20160805170050164.jpg','冒险解谜',1,'HTC Vive|Oculus CV1','手柄|键鼠','http://resource.vr-store.cn/appfile/c68fd94a31594f91a2fb95c2c7cccb45.zip',10,'145.77M',10.0,'591vr','<p>新版《歌罗西》兼容Oculus Rift消费版头显和HTC Vive场景以观众为主导的故事叙述，故事围绕遗失了很久的伟大人物展开，它探索了恐惧、权利和尊重等主题。这些人物是以设计复杂的巨人形象出现，场景中没有人物对话，只有一个老人的独白叙述着整个故事。</p><p><img src="./game/details/b/20160805170050165.png"></p><p><img src="./game/details/b/20160805170050166.jpg"></p>');
INSERT INTO `game`(title, create_time, company, preinfo, preimage, type, system, display_device, control_device, download_url, download_num, installation_size, score, source, content) VALUES ('俩姐妹','2016-05-11 00:00:00',318,'小心，这里没你想象的那么安全；随处，都可能隐藏着你最不想看到的东西！揭开这件密室的阴谋，拯救你的姐妹，我想你也不想一直呆在这里吧，那就利用你的聪明才智，尽早逃离这里吧。注：此款游戏支持HTC Vive设备，可以在2米*2米以上的范围体验，不过这款目前是试玩版本的单人游戏。','./game/preImages/b/20160805170050167.jpg','冒险解谜',1,'HTC Vive','手柄|键鼠','http://resource.vr-store.cn/appfile/529f81edf9e5429ab9b35377718a0d88.zip',10,'108.57M',10.0,'591vr','<p>小心，这里没你想象的那么安全；随处，都可能隐藏着你最不想看到的东西！揭开这件密室的阴谋，拯救你的姐妹，我想你也不想一直呆在这里吧，那就利用你的聪明才智，尽早逃离这里吧。注：此款游戏支持HTC Vive设备，可以在2米*2米以上的范围体验，不过这款目前是试玩版本的单人游戏。</p><p><img src="./game/details/b/20160805170050168.jpg"></p><p><img src="./game/details/b/20160805170050169.jpg"></p>');
INSERT INTO `game`(title, create_time, company, preinfo, preimage, type, system, display_device, control_device, download_url, download_num, installation_size, score, source, content) VALUES ('飞船跑酷','2016-06-21 00:00:00',319,'进入外太空中，你必须以极快的速度、闪电般的反应能力来获取高分。作为一名星际战斗机飞行员，你能够活多久？','./game/preImages/b/20160805170050170.jpg','冒险解谜',1,'Oculus DK1|Oculus DK2|Deepoon|HTC Vive','手柄|键鼠','http://resource.vr-store.cn/appfile/f9d163852c1b4319bbe875b50bce4330.rar',10,'44.13M',10.0,'591vr','<p>进入外太空中，你必须以极快的速度、闪电般的反应能力来获取高分。作为一名星际战斗机飞行员，你能够活多久？</p><p><img src="./game/details/b/20160805170050171.jpg"></p><p><img src="./game/details/b/20160805170050172.jpg"></p><p><img src="./game/details/b/20160805170050173.jpg"></p>');
INSERT INTO `game`(title, create_time, company, preinfo, preimage, type, system, display_device, control_device, download_url, download_num, installation_size, score, source, content) VALUES ('普朗克能源基地','2016-05-09 00:00:00',320,'普朗克国际能源基地，这里是世界上最大的量子反应堆的地下设施之一，庞大的基地和历史悠久的有利条件，让这里充满神秘黑能量。玩家驾驶“深空号”深入探险这里的秘密，你还可以使用盾牌、反冲激光手枪等武器进行自我保护。此游戏属于试玩版本！发布者表示正式版本会在今年夏季发布，喜欢这款游戏的玩家敬请期待591VR官网，我们会及时发布！','./game/preImages/b/20160805170050174.jpg','第一人称射击',1,'HTC Vive','手柄|键鼠','http://resource.vr-store.cn/appfile/8bbd02a36622463e98b6c2b3d26b9892.zip',10,'106.07M',10.0,'591vr','<p>普朗克国际能源基地，这里是世界上最大的量子反应堆的地下设施之一，庞大的基地和历史悠久的有利条件，让这里充满神秘黑能量。玩家驾驶“深空号”深入探险这里的秘密，你还可以使用盾牌、反冲激光手枪等武器进行自我保护。此游戏属于试玩版本！发布者表示正式版本会在今年夏季发布，喜欢这款游戏的玩家敬请期待591VR官网，我们会及时发布！</p><p><img src="./game/details/b/20160805170050175.jpg"></p><p><img src="./game/details/b/20160805170050176.jpg"></p><p><img src="./game/details/b/20160805170050177.jpg"></p>');
INSERT INTO `game`(title, create_time, company, preinfo, preimage, type, system, display_device, control_device, download_url, download_num, installation_size, score, source, content) VALUES ('沉思的宇宙二','2016-05-11 00:00:00',321,'更新版本的《沉思的宇宙二（Doll City VR）》是一款支持HTC Vive的VR体验。它的原型是the Cyberpunk universe of “Potentiel”，经改编重造出这个独特的VR体验。基于图形和声音的宇宙，传输纯粹的迷幻谵妄，科幻感非常强，值得一玩！','./game/preImages/b/20160805170051178.jpg','漫游体验',1,'HTC Vive','手柄|键鼠','http://resource.vr-store.cn/appfile/thinktheallthingtwo.zip',10,'1.22G',10.0,'591vr','<p>更新版本的《沉思的宇宙二（Doll City VR）》是一款支持HTC Vive的VR体验。它的原型是the Cyberpunk universe of “Potentiel”，经改编重造出这个独特的VR体验。基于图形和声音的宇宙，传输纯粹的迷幻谵妄，科幻感非常强，值得一玩！</p><p><img src="./game/details/b/20160805170051179.jpg"></p><p><img src="./game/details/b/20160805170051180.jpg"></p>');
INSERT INTO `game`(title, create_time, company, preinfo, preimage, type, system, display_device, control_device, download_url, download_num, installation_size, score, source, content) VALUES ('死亡过山车','2016-06-23 00:00:00',611,'非常有趣的过山车游戏一枚!如果你觉得所有的过山车游戏都是坐在座位上四处看看惊声尖叫一下，那你就大错特错了!这款过山车的有趣之处在于，要想到达终点，你需要控制你的过山车选取正确的轨道。','./game/preImages/b/20160805170051181.png','冒险解谜',1,'Oculus DK1|Oculus DK2|Deepoon','','http://resource.vr-store.cn/appfile/6c9db711ffdf41dbb1c4c4981b1e0459.zip',10,'78.18M',10.0,'591vr','<p>非常有趣的过山车游戏一枚!如果你觉得所有的过山车游戏都是坐在座位上四处看看惊声尖叫一下，那你就大错特错了!这款过山车的有趣之处在于，要想到达终点，你需要控制你的过山车选取正确的轨道。</p><p><img src="./game/details/b/20160805170051182.jpg"></p><p><img src="./game/details/b/20160805170051183.jpg"></p><p><img src="./game/details/b/20160805170051184.jpg"></p>');
INSERT INTO `game`(title, create_time, company, preinfo, preimage, type, system, display_device, control_device, download_url, download_num, installation_size, score, source, content) VALUES ('棒球运动','2016-04-08 00:00:00',322,'这是一款支持HTC Vive 的运动游戏，玩家确保在空间2米x1.5米的范围内可以自由施展自己的运动才能。拿起你的手柄，它就是你的棒杆，按键T即可选择小球。在这里无规则无限制，玩家可以随意进行射击这里的任何事物。当然，搞破坏是一件很释压的事不是吗？','./game/preImages/b/20160805170051185.jpg','动作游戏',1,'HTC Vive','手柄|键鼠','http://resource.vr-store.cn/appfile/1e659d66c4844cea9e0976ba633107fc.zip',10,'309.4M',10.0,'591vr','<p>这是一款支持HTC Vive 的运动游戏，玩家确保在空间2米x1.5米的范围内可以自由施展自己的运动才能。拿起你的手柄，它就是你的棒杆，按键T即可选择小球。在这里无规则无限制，玩家可以随意进行射击这里的任何事物。当然，搞破坏是一件很释压的事不是吗？</p><p><img src="./game/details/b/20160805170051186.jpg"></p><p><img src="./game/details/b/20160805170051187.jpg"></p><p><img src="./game/details/b/20160805170051188.jpg"></p>');
INSERT INTO `game`(title, create_time, company, preinfo, preimage, type, system, display_device, control_device, download_url, download_num, installation_size, score, source, content) VALUES ('急速卡通赛车','2016-04-29 00:00:00',323,'此版本属于试玩版游戏，安装需注意事项：在桌面创建快捷方式，并修改启动参数，在“目标”里加上-VR，否则不会在Oculus DK2里启动，只会是普通得全屏模式 。样式：“TOYBOXTURBOS.EXE -VR”','./game/preImages/b/20160805170051189.jpg','赛车竞速',1,'Oculus DK1|Oculus DK2|Deepoon','键鼠','http://resource.vr-store.cn/appfile/e0fa1c4c168741bfa2ee641e2a3ad952.zip',10,'429.68M',10.0,'591vr','<p>此版本属于试玩版游戏，安装需注意事项：在桌面创建快捷方式，并修改启动参数，在“目标”里加上-VR，否则不会在Oculus DK2里启动，只会是普通得全屏模式 。样式：“TOYBOXTURBOS.EXE -VR”</p><p><img src="./game/details/b/20160805170051190.jpg"></p><p><img src="./game/details/b/20160805170051191.jpg"></p><p><img src="./game/details/b/20160805170051192.jpg"></p>');
INSERT INTO `game`(title, create_time, company, preinfo, preimage, type, system, display_device, control_device, download_url, download_num, installation_size, score, source, content) VALUES ('迷失旅程','2016-04-27 00:00:00',324,'第一人称的闯关探险游戏，由于航线发生偏离，玩家莫名其妙的被带到一个神秘的世界，这里荒无人烟，想要逃离可没那么简单。','./game/preImages/b/20160805170052193.jpg','冒险解谜',1,'Oculus DK2|Oculus CV1','手柄|键鼠','http://resource.vr-store.cn/appfile/4f2fafef4cc94188aceff85a8ac0912b.rar',10,'104.76M',10.0,'591vr','<p>第一人称的闯关探险游戏，由于航线发生偏离，玩家莫名其妙的被带到一个神秘的世界，这里荒无人烟，想要逃离可没那么简单。</p><p><img src="./game/details/b/20160805170052194.jpg"></p><p><img src="./game/details/b/20160805170052195.jpg"></p><p><img src="./game/details/b/20160805170052196.jpg"></p>');
INSERT INTO `game`(title, create_time, company, preinfo, preimage, type, system, display_device, control_device, download_url, download_num, installation_size, score, source, content) VALUES ('银河碰碰球','2016-04-05 00:00:00',325,'玩家扮演一个小红球，你很爱钻石，想必你已经知道该怎么玩这个游戏了！收集所有的钻石可以进入下一个游戏场景；看到有diamont前往别错过，收集它你可以获得抽金球的机会；小心，不能让黑球触屏到你，否则它会偷走你所有的东西；记住，你只是一个小球，你不能飞，所以前往小心，别掉下去！','./game/preImages/b/20160805170052197.jpg','休闲益智',1,'Oculus DK2|Deepoon','键鼠','http://resource.vr-store.cn/appfile/dd12a455c6124d7b9c5502835016239c.zip',10,'45.03M',10.0,'591vr','<p>玩家扮演一个小红球，你很爱钻石，想必你已经知道该怎么玩这个游戏了！收集所有的钻石可以进入下一个游戏场景；看到有diamont前往别错过，收集它你可以获得抽金球的机会；小心，不能让黑球触屏到你，否则它会偷走你所有的东西；记住，你只是一个小球，你不能飞，所以前往小心，别掉下去！</p><p><img src="./game/details/b/20160805170052198.jpg"></p>');
INSERT INTO `game`(title, create_time, company, preinfo, preimage, type, system, display_device, control_device, download_url, download_num, installation_size, score, source, content) VALUES ('头球运动','2016-04-13 00:00:00',326,'都说玩游戏伤身体，今天小编给大家带来一款打破常规的游戏，让各位V友们在玩耍中强身健体。听名字大家应该能猜想出来，这款游戏是用头部来击打球，通过打球的次数来获得更高的积分。','./game/preImages/b/20160805170052199.jpg','动作游戏',1,'Oculus DK2|Deepoon','手柄|键鼠','http://resource.vr-store.cn/appfile/5cb84c668f48426aadc7956412bc106d.zip',10,'159.23M',10.0,'591vr','<p>都说玩游戏伤身体，今天小编给大家带来一款打破常规的游戏，让各位V友们在玩耍中强身健体。听名字大家应该能猜想出来，这款游戏是用头部来击打球，通过打球的次数来获得更高的积分。</p><p><img src="./game/details/b/20160805170052200.jpg"></p>');
INSERT INTO `game`(title, create_time, company, preinfo, preimage, type, system, display_device, control_device, download_url, download_num, installation_size, score, source, content) VALUES ('光线摩托','2016-03-24 00:00:00',327,'这款光线摩托是不是看着很眼熟？没错，这是根据《创世纪》电影改编而来的VR游戏，驾驶炫酷的摩托在赛道上飞驰一把，一定非常过瘾！','./game/preImages/b/20160805170052201.jpg','赛车竞速',1,'Oculus DK1|Oculus DK2|Deepoon','手柄|键鼠','http://resource.vr-store.cn/appfile/5b89beddf1b04854a9a6e23692f39dfd.zip',10,'38.68M',10.0,'591vr','<p>这款光线摩托是不是看着很眼熟？没错，这是根据《创世纪》电影改编而来的VR游戏，驾驶炫酷的摩托在赛道上飞驰一把，一定非常过瘾！</p><p><img src="./game/details/b/20160805170052202.png"></p><p><img src="./game/details/b/20160805170052203.png"></p>');
INSERT INTO `game`(title, create_time, company, preinfo, preimage, type, system, display_device, control_device, download_url, download_num, installation_size, score, source, content) VALUES ('龙猫','2016-05-04 00:00:00',331,'此款游戏是最新版本《龙猫VR》，支持HTC Vive控制器！在这个怀旧VR应用中，你将会穿越到《龙猫》这部经典电影的场景之中，你看到雨伞是可以点击鼠标（左键）拿起雨伞，走过一段长长的雨夜土路，你会在雨夜的公交站旁碰上龙猫，把伞给它后（左键）和他一起等待巴士吧！','./game/preImages/b/20160805170053204.jpg','休闲益智',1,'HTC Vive','手柄','http://resource.vr-store.cn/appfile/685d400b047e4758ba77b2e05ba4cd42.zip',10,'207.65M',10.0,'591vr','<p>此款游戏是最新版本《龙猫VR》，支持HTC Vive控制器！在这个怀旧VR应用中，你将会穿越到《龙猫》这部经典电影的场景之中，你看到雨伞是可以点击鼠标（左键）拿起雨伞，走过一段长长的雨夜土路，你会在雨夜的公交站旁碰上龙猫，把伞给它后（左键）和他一起等待巴士吧！</p><p><img src="./game/details/b/20160805170053205.jpg"></p><p><img src="./game/details/b/20160805170053206.jpg"></p><p><img src="./game/details/b/20160805170053207.jpg"></p>');
INSERT INTO `game`(title, create_time, company, preinfo, preimage, type, system, display_device, control_device, download_url, download_num, installation_size, score, source, content) VALUES ('Kvorak大冒险','2016-04-12 00:00:00',332,'欢迎来到医生Kvorak的躲避游戏，你的目的是控制游戏中的三个选手，拿到水晶球。在这里没有什么是不可能的，只要你赢了，你就可以掌握这个世界。此游戏还处于试玩阶段，想要体验正式版本，尽情关注591VR，我们会第一时间更新游戏。','./game/preImages/b/20160805170053208.jpg','冒险解谜',1,'HTC Vive','手柄|键鼠','http://resource.vr-store.cn/appfile/189308f041064c5c894e69b5fb94b925.zip',10,'512.35M',10.0,'591vr','<p>欢迎来到医生Kvorak的躲避游戏，你的目的是控制游戏中的三个选手，拿到水晶球。在这里没有什么是不可能的，只要你赢了，你就可以掌握这个世界。此游戏还处于试玩阶段，想要体验正式版本，尽情关注591VR，我们会第一时间更新游戏。</p><p><img src="./game/details/b/20160805170053209.jpg"></p><p><img src="./game/details/b/20160805170053210.jpg"></p><p><img src="./game/details/b/20160805170053211.jpg"></p>');
INSERT INTO `game`(title, create_time, company, preinfo, preimage, type, system, display_device, control_device, download_url, download_num, installation_size, score, source, content) VALUES ('懒人办公室','2016-04-08 00:00:00',297,'文件、电脑以及其他办公用品都呈现在这个普通的办公室里面，了解这里的工作环境，为这里整理办公文件，打造一个舒适整洁的工作氛围。','./game/preImages/b/20160805170053212.jpg','休闲益智',1,'HTC Vive','手柄|键鼠','http://resource.vr-store.cn/appfile/e98110735bf94707bfdbfc0725c7b3f0.zip',10,'67.03M',10.0,'591vr','<p>文件、电脑以及其他办公用品都呈现在这个普通的办公室里面，了解这里的工作环境，为这里整理办公文件，打造一个舒适整洁的工作氛围。</p><p><img src="./game/details/b/20160805170053213.jpg"></p><p><img src="./game/details/b/20160805170053214.jpg"></p><p><img src="./game/details/b/20160805170053215.jpg"></p>');
INSERT INTO `game`(title, create_time, company, preinfo, preimage, type, system, display_device, control_device, download_url, download_num, installation_size, score, source, content) VALUES ('迷你高尔夫','2016-04-19 00:00:00',107,'体验一场在高端云层的优美风景区中玩耍高尔夫的乐趣，这是一个单人试玩版游戏，如果你喜欢可以期待即将推出的本地多人版本。','./game/preImages/b/20160805170053216.jpg','动作游戏',1,'HTC Vive','手柄|键鼠','http://resource.vr-store.cn/appfile/8a0302fd0e874df68c9c715129762a53.zip',10,'258.68M',10.0,'591vr','<p>体验一场在高端云层的优美风景区中玩耍高尔夫的乐趣，这是一个单人试玩版游戏，如果你喜欢可以期待即将推出的本地多人版本。</p><p><img src="./game/details/b/20160805170053217.jpg"></p><p><img src="./game/details/b/20160805170053218.jpg"></p><p><img src="./game/details/b/20160805170053219.jpg"></p>');
INSERT INTO `game`(title, create_time, company, preinfo, preimage, type, system, display_device, control_device, download_url, download_num, installation_size, score, source, content) VALUES ('非理性繁荣：序幕','2016-04-08 00:00:00',333,'《非理性繁荣》是探测大规模的一款VR游戏，玩家可以在这里充分展示自己的想象力和深空进行高度互动。序幕是银河幻想的前端，玩家在这里唤醒沉睡在深空中的彗星和其他小型行星。每一步都充满科幻与冒险，喜欢的玩家不容错过！','./game/preImages/b/20160805170054220.jpg','冒险解谜',1,'HTC Vive','手柄|键鼠','http://resource.vr-store.cn/appfile/121e55ff5b3742848c06db54f3c6df4f.zip',10,'99.42M',10.0,'591vr','<p>《非理性繁荣》是探测大规模的一款VR游戏，玩家可以在这里充分展示自己的想象力和深空进行高度互动。序幕是银河幻想的前端，玩家在这里唤醒沉睡在深空中的彗星和其他小型行星。每一步都充满科幻与冒险，喜欢的玩家不容错过！</p><p><img src="./game/details/b/20160805170054221.jpg"></p><p><img src="./game/details/b/20160805170054222.jpg"></p><p><img src="./game/details/b/20160805170054223.jpg"></p>');
INSERT INTO `game`(title, create_time, company, preinfo, preimage, type, system, display_device, control_device, download_url, download_num, installation_size, score, source, content) VALUES ('道士天书：诅咒','2016-04-05 00:00:00',334,'1680年，意大利Vicenza一个古老的宗教——里德尔的庄园，表面的高贵和美丽，却到处充满阴谋和黑暗。历代传承下来的天书，不料是一个阴险的诡计，一直诅咒着生活在这里的家庭。探秘这里不为人知的秘密，拯救这里无辜受累的百姓。','./game/preImages/b/20160805170054224.jpg','冒险解谜',1,'HTC Vive','手柄|键鼠','http://resource.vr-store.cn/appfile/d73f3bbbb687484798ca964745729152.zip',10,'542.29M',10.0,'591vr','<p>1680年，意大利Vicenza一个古老的宗教——里德尔的庄园，表面的高贵和美丽，却到处充满阴谋和黑暗。历代传承下来的天书，不料是一个阴险的诡计，一直诅咒着生活在这里的家庭。探秘这里不为人知的秘密，拯救这里无辜受累的百姓。</p><p><img src="./game/details/b/20160805170054225.jpg"></p><p><img src="./game/details/b/20160805170054226.jpg"></p><p><img src="./game/details/b/20160805170054227.jpg"></p>');
INSERT INTO `game`(title, create_time, company, preinfo, preimage, type, system, display_device, control_device, download_url, download_num, installation_size, score, source, content) VALUES ('天空战斗机','2016-03-21 00:00:00',157,'今天是你训练的第一天，注意千万不要辜负你导师的信任栽培。你必须要学会作为天空战士的盔甲配备的四个基本系统：推动、保卫、防御、射击，努力完成你的训练，可别破坏了你的铠甲。','./game/preImages/b/20160805170054228.jpg','第一人称射击',1,'Oculus DK1|Oculus DK2|Deepoon','手柄|键鼠','http://resource.vr-store.cn/appfile/c1f73a4aa6484dcd88c7b0b3298db2b8.zip',10,'119.69M',10.0,'591vr','<p>今天是你训练的第一天，注意千万不要辜负你导师的信任栽培。你必须要学会作为天空战士的盔甲配备的四个基本系统：推动、保卫、防御、射击，努力完成你的训练，可别破坏了你的铠甲。</p><p><img src="./game/details/b/20160805170054229.jpg"></p><p><img src="./game/details/b/20160805170054230.jpg"></p><p><img src="./game/details/b/20160805170054231.jpg"></p>');
INSERT INTO `game`(title, create_time, company, preinfo, preimage, type, system, display_device, control_device, download_url, download_num, installation_size, score, source, content) VALUES ('削减预算','2016-04-20 00:00:00',335,'《削减预算》是首款在游戏中设置中加入传送门运动技术的VR游戏之一，在这里玩家和你的手下潜入敌军秘密基地，使用传送抢、匕首将敌人击退。游戏场景中设有机器人邵兵，这需要玩家使用手中的传送抢躲过哨兵的巡查，所以玩家拥有敏感的方向性是成功的关键。目前这款还是试玩游戏，完整版将于今年年底发布，我们会第一时间为玩家公布。','./game/preImages/b/20160805170054232.jpg','动作游戏',1,'HTC Vive','手柄|键鼠','http://resource.vr-store.cn/appfile/020a25002b3d40fc83fbdb0f8cca6888.zip',10,'120.47M',10.0,'591vr','<p>《削减预算》是首款在游戏中设置中加入传送门运动技术的VR游戏之一，在这里玩家和你的手下潜入敌军秘密基地，使用传送抢、匕首将敌人击退。游戏场景中设有机器人邵兵，这需要玩家使用手中的传送抢躲过哨兵的巡查，所以玩家拥有敏感的方向性是成功的关键。目前这款还是试玩游戏，完整版将于今年年底发布，我们会第一时间为玩家公布。</p><p><img src="./game/details/b/20160805170054233.png"></p><p><img src="./game/details/b/20160805170054234.jpg"></p><p><img src="./game/details/b/20160805170054235.png"></p>');
INSERT INTO `game`(title, create_time, company, preinfo, preimage, type, system, display_device, control_device, download_url, download_num, installation_size, score, source, content) VALUES ('Jeeboman','2016-04-15 00:00:00',336,'《Jeeboman》圆你一场英雄梦，手持追踪遥控器，左手防卫右手攻击，射击来自外星的邪恶机器人，保卫你的城市。該版本是试玩版游戏！','./game/preImages/b/20160805170054236.jpg','漫游体验',1,'HTC Vive','手柄|键鼠','http://resource.vr-store.cn/appfile/5f2ff4661d164219b08a0ec199ec0572.zip',10,'298.11M',10.0,'591vr','<p>《Jeeboman》圆你一场英雄梦，手持追踪遥控器，左手防卫右手攻击，射击来自外星的邪恶机器人，保卫你的城市。該版本是试玩版游戏！</p><p><img src="./game/details/b/20160805170054237.jpg"></p><p><img src="./game/details/b/20160805170054238.jpg"></p><p><img src="./game/details/b/20160805170054239.jpg"></p>');
INSERT INTO `game`(title, create_time, company, preinfo, preimage, type, system, display_device, control_device, download_url, download_num, installation_size, score, source, content) VALUES ('魔幻实验室','2016-04-08 00:00:00',337,'欢迎来到《魔幻实验室》，一个由 Valve 建立在光圈科技口袋宇宙内的房间规模 VR 实验合集。游戏内容中包含「Slingshot」：以校准培训生的身分开始您的职涯，利用核心校准弹弓鲁莽地破坏光圈科技储藏附属仓库内的一切。「Longbow」：将弓术发挥到极致来保卫您的城门，抵抗来自暴走却依旧可爱的部落攻击。「Xortex」：宇宙飞船射击游戏，体验者手上操控宇宙飞船往各种方向移动，而敌人会进行弹幕射击。「Postcards」：让人身处在充满异国风情的偏远地点。「Human Medical Scan」：透过断层扫描还原的精致模型来探索人体的奥妙。','./game/preImages/b/20160805170055240.jpg','漫游体验',1,'HTC Vive','手柄|键鼠','http://resource.vr-store.cn/appfile/mohuanshiyanshi.zip',10,'7.06G',10.0,'591vr','<p>欢迎来到《魔幻实验室》，一个由 Valve 建立在光圈科技口袋宇宙内的房间规模 VR 实验合集。游戏内容中包含「Slingshot」：以校准培训生的身分开始您的职涯，利用核心校准弹弓鲁莽地破坏光圈科技储藏附属仓库内的一切。「Longbow」：将弓术发挥到极致来保卫您的城门，抵抗来自暴走却依旧可爱的部落攻击。「Xortex」：宇宙飞船射击游戏，体验者手上操控宇宙飞船往各种方向移动，而敌人会进行弹幕射击。「Postcards」：让人身处在充满异国风情的偏远地点。「Human Medical Scan」：透过断层扫描还原的精致模型来探索人体的奥妙。</p><p><img src="./game/details/b/20160805170055241.jpg"></p><p><img src="./game/details/b/20160805170055242.jpg"></p><p><img src="./game/details/b/20160805170055243.jpg"></p>');
INSERT INTO `game`(title, create_time, company, preinfo, preimage, type, system, display_device, control_device, download_url, download_num, installation_size, score, source, content) VALUES ('宜家VR体验','2016-04-08 00:00:00',338,'这款宜家VR体验，把Vive用户带到1:1的真实世界场景的虚拟宜家厨房，用户可以在这里随意改变厨房橱柜和抽屉颜色，知道你满意为止。','./game/preImages/b/20160805170055244.jpg','漫游体验',1,'HTC Vive','手柄|键鼠','http://resource.vr-store.cn/appfile/f281ebcfc0bf49dfa2d8dbf4767e0dfa.zip',10,'619.11M',10.0,'591vr','<p>这款宜家VR体验，把Vive用户带到1:1的真实世界场景的虚拟宜家厨房，用户可以在这里随意改变厨房橱柜和抽屉颜色，知道你满意为止。</p><p><img src="./game/details/b/20160805170055245.jpg"></p><p><img src="./game/details/b/20160805170055246.jpg"></p><p><img src="./game/details/b/20160805170055247.jpg"></p>');
INSERT INTO `game`(title, create_time, company, preinfo, preimage, type, system, display_device, control_device, download_url, download_num, installation_size, score, source, content) VALUES ('睡神之旅','2016-03-21 00:00:00',339,'有严重失眠的朋友们可以通过这款游戏，体验到清醒梦境的一种实验性治疗。同时这也是一款考验玩家智力的游戏，场景效果虽不够精美，但既是益智，小编在此就不透露太多细节，有兴趣的朋友们预祝你们顺利闯关成功！','./game/preImages/b/20160805170055248.jpg','休闲益智',1,'Oculus DK2|Deepoon','手柄|键鼠','http://resource.vr-store.cn/appfile/e09bdeca0648447bb34be1ef1436cdea.zip',10,'342.78M',10.0,'591vr','<p>有严重失眠的朋友们可以通过这款游戏，体验到清醒梦境的一种实验性治疗。同时这也是一款考验玩家智力的游戏，场景效果虽不够精美，但既是益智，小编在此就不透露太多细节，有兴趣的朋友们预祝你们顺利闯关成功！</p><p><img src="./game/details/b/20160805170055249.jpg"></p><p><img src="./game/details/b/20160805170055250.jpg"></p><p><img src="./game/details/b/20160805170055251.jpg"></p>');
INSERT INTO `game`(title, create_time, company, preinfo, preimage, type, system, display_device, control_device, download_url, download_num, installation_size, score, source, content) VALUES ('寿司过山车','2016-04-12 00:00:00',340,'支持驱动Oculus Rumtime 1.3.0的游戏来了，喜欢玩过山车的吃货朋友可以体验一下，自己化身成为寿司在过山车上飞驰，不过你不会被吃掉，所以尽情享受这个刺激与乐趣吧！','./game/preImages/b/20160805170055252.jpg','冒险解谜',1,'Oculus DK2|Deepoon','键鼠','http://resource.vr-store.cn/appfile/19993704d1724dc0851708bc6353799f.zip',10,'84.26M',10.0,'591vr','<p>支持驱动Oculus Rumtime 1.3.0的游戏来了，喜欢玩过山车的吃货朋友可以体验一下，自己化身成为寿司在过山车上飞驰，不过你不会被吃掉，所以尽情享受这个刺激与乐趣吧！</p><p><img src="./game/details/b/20160805170055253.jpg"></p><p><img src="./game/details/b/20160805170055254.jpg"></p><p><img src="./game/details/b/20160805170055255.jpg"></p>');
INSERT INTO `game`(title, create_time, company, preinfo, preimage, type, system, display_device, control_device, download_url, download_num, installation_size, score, source, content) VALUES ('奇观炫境','2016-04-08 00:00:00',341,'戴上你的Vive设备，世界上最迷人地方便在你眼前。在这里，你可以选择地球上任何一个点，只需轻按按钮，你就可以去到那里！','./game/preImages/b/20160805170055256.jpg','漫游体验',1,'HTC Vive','手柄|键鼠','http://resource.vr-store.cn/appfile/qiguanxuanjing.zip',10,'7.8G',10.0,'591vr','<p>戴上你的Vive设备，世界上最迷人地方便在你眼前。在这里，你可以选择地球上任何一个点，只需轻按按钮，你就可以去到那里！</p><p><img src="./game/details/b/20160805170055257.jpg"></p><p><img src="./game/details/b/20160805170055258.jpg"></p><p><img src="./game/details/b/20160805170055259.jpg"></p>');
INSERT INTO `game`(title, create_time, company, preinfo, preimage, type, system, display_device, control_device, download_url, download_num, installation_size, score, source, content) VALUES ('恶魔之夜','2016-02-24 00:00:00',343,'8岁的托马斯刚搬进他的新房子;他的父母抢购交易的财产没有意识到它黑暗的过去。托马斯发现了一个立体声播放器留在车内的磁带，和一个手电筒......夜幕降临的时候，他很快意识到他不是一个人在房间里...磁带开始播放……注：此款游戏属于试玩版！','./game/preImages/b/20160805170056260.jpg','动作游戏',1,'3Glasses|Oculus DK1|Oculus DK2|Deepoon|3Glasses','键鼠','http://resource.vr-store.cn/appfile/d00e19a687a34a71a9d9405e8ef011e4.zip',10,'539.16M',10.0,'591vr','<p>8岁的托马斯刚搬进他的新房子;他的父母抢购交易的财产没有意识到它黑暗的过去。托马斯发现了一个立体声播放器留在车内的磁带，和一个手电筒......夜幕降临的时候，他很快意识到他不是一个人在房间里...磁带开始播放……注：此款游戏属于试玩版！</p><p><img src="./game/details/b/20160805170056261.jpg"></p><p><img src="./game/details/b/20160805170056262.jpg"></p><p><img src="./game/details/b/20160805170056263.jpg"></p>');
INSERT INTO `game`(title, create_time, company, preinfo, preimage, type, system, display_device, control_device, download_url, download_num, installation_size, score, source, content) VALUES ('质子脉冲','2016-02-24 00:00:00',344,'《质子脉冲》是一款快节奏的第三人称打砖块动作游戏，专为虚拟现实开发而成。游戏中玩家将在一个3D管道内击打不断反弹回来的质子球，质子球在反弹过程中碰到砖块即可得分。对于不习惯使用摇杆和键鼠操作的发烧友们，这款以头盔设备作为输入方式，无疑是福音啊！注：此款游戏属于试玩版！','./game/preImages/b/20160805170056264.jpg','动作游戏',1,'3Glasses|Oculus DK1|Oculus DK2|Deepoon|3Glasses','键鼠','http://resource.vr-store.cn/appfile/16bf21e082da4894ade11956b58dffda.zip',10,'87.89M',10.0,'591vr','<p>《质子脉冲》是一款快节奏的第三人称打砖块动作游戏，专为虚拟现实开发而成。游戏中玩家将在一个3D管道内击打不断反弹回来的质子球，质子球在反弹过程中碰到砖块即可得分。对于不习惯使用摇杆和键鼠操作的发烧友们，这款以头盔设备作为输入方式，无疑是福音啊！注：此款游戏属于试玩版！</p><p><img src="./game/details/b/20160805170056265.jpg"></p><p><img src="./game/details/b/20160805170056266.jpg"></p><p><img src="./game/details/b/20160805170056267.jpg"></p>');
INSERT INTO `game`(title, create_time, company, preinfo, preimage, type, system, display_device, control_device, download_url, download_num, installation_size, score, source, content) VALUES ('小小飞行员','2016-02-23 00:00:00',345,'非常有趣的Oculus Rift虚拟现实冒险游戏，在场景中玩家扮演的是一名勤劳的飞行员，在轨道上驾驶着你的飞行车接送客人。不过别把事情想的太过简单，你要面对的是自己的破旧小车，不断下降的油量和手握激光炮的外星人。游戏难度不低，非常有挑战性，场景一共有四个关卡，好好把握吧！','./game/preImages/b/20160805170056268.jpg','漫游体验',1,'Oculus DK2','手柄|键鼠','http://resource.vr-store.cn/appfile/8e9f4795ec9f4e7e9f1ae67fe4891afd.zip',10,'427.88M',10.0,'591vr','<p>非常有趣的Oculus Rift虚拟现实冒险游戏，在场景中玩家扮演的是一名勤劳的飞行员，在轨道上驾驶着你的飞行车接送客人。不过别把事情想的太过简单，你要面对的是自己的破旧小车，不断下降的油量和手握激光炮的外星人。游戏难度不低，非常有挑战性，场景一共有四个关卡，好好把握吧！</p><p><img src="./game/details/b/20160805170056269.jpg"></p><p><img src="./game/details/b/20160805170056270.jpg"></p><p><img src="./game/details/b/20160805170056271.png"></p>');
INSERT INTO `game`(title, create_time, company, preinfo, preimage, type, system, display_device, control_device, download_url, download_num, installation_size, score, source, content) VALUES ('细胞空间','2016-02-25 00:00:00',346,'玩家扮演首个成功微缩的人类，将被注射到另一个人体内来治疗疾病，首要任务是通过直接运送疫苗到受感染的细胞核来治愈一位流行性感冒患者。','./game/preImages/b/20160805170057272.jpg','漫游体验',1,'Oculus DK1|Oculus DK2|Deepoon|HTC Vive','手柄|键鼠','http://resource.vr-store.cn/appfile/c02050f1602d496fb0afd11ce0a71210.zip',10,'633.89M',10.0,'591vr','<p>玩家扮演首个成功微缩的人类，将被注射到另一个人体内来治疗疾病，首要任务是通过直接运送疫苗到受感染的细胞核来治愈一位流行性感冒患者。</p><p><img src="./game/details/b/20160805170057273.png"></p><p><img src="./game/details/b/20160805170057274.png"></p><p><img src="./game/details/b/20160805170057275.png"></p>');
INSERT INTO `game`(title, create_time, company, preinfo, preimage, type, system, display_device, control_device, download_url, download_num, installation_size, score, source, content) VALUES ('SOLRAVEN','2016-03-10 00:00:00',347,'您是共和国最优秀的飞行员。你的任务是控制导弹攻击敌方飞船。游戏操作简单，利用鼠标控制导弹发射即可，无需控制飞行，非常适合初级射击爱好者练习。注：鼠标右键：火导弹；鼠标左键：消防枪','./game/preImages/b/20160805170057276.jpg','第一人称射击',1,'Oculus DK2|Deepoon','键鼠','http://resource.vr-store.cn/appfile/3c1785790dd84fd886f837077c25a535.zip',10,'866.25M',10.0,'591vr','<p>您是共和国最优秀的飞行员。你的任务是控制导弹攻击敌方飞船。游戏操作简单，利用鼠标控制导弹发射即可，无需控制飞行，非常适合初级射击爱好者练习。注：鼠标右键：火导弹；鼠标左键：消防枪</p><p><img src="./game/details/b/20160805170057277.png"></p><p><img src="./game/details/b/20160805170057278.jpg"></p><p><img src="./game/details/b/20160805170057279.png"></p>');
INSERT INTO `game`(title, create_time, company, preinfo, preimage, type, system, display_device, control_device, download_url, download_num, installation_size, score, source, content) VALUES ('夜深人静','2016-02-29 00:00:00',348,'这款有12岁的CBGames利用Unity5引擎开发的VR恐怖游戏，听起来是不是很不可思议？小编12岁的时候才刚开始玩游戏，真是人比人吓死人啊！！这世道还有天理吗？话说这是有小孩子开发的游戏，但它在VR恐怖游戏中还是非常好玩的，足以吓得玩家们惊悚慌张。赶紧体验一下这位天才为我们制作的恐怖片吧！','./game/preImages/b/20160805170057280.jpg','恐怖游戏',1,'Oculus DK2|Deepoon','键鼠','http://resource.vr-store.cn/appfile/c0ab8507157a46d0a5ffa977c948d13c.zip',10,'222.78M',10.0,'591vr','<p>这款有12岁的CBGames利用Unity5引擎开发的VR恐怖游戏，听起来是不是很不可思议？小编12岁的时候才刚开始玩游戏，真是人比人吓死人啊！！这世道还有天理吗？话说这是有小孩子开发的游戏，但它在VR恐怖游戏中还是非常好玩的，足以吓得玩家们惊悚慌张。赶紧体验一下这位天才为我们制作的恐怖片吧！</p><p><img src="./game/details/b/20160805170057281.png"></p><p><img src="./game/details/b/20160805170057282.png"></p>');
INSERT INTO `game`(title, create_time, company, preinfo, preimage, type, system, display_device, control_device, download_url, download_num, installation_size, score, source, content) VALUES ('布鲁克海文实验室','2016-03-03 00:00:00',349,'好消息，大好消息！HTC射手游戏重磅来袭，拥有VIVE的发烧友们再也不用担心没有好玩的VR游戏啦！在此款游戏中世界的终结时间即将来临，玩家务必需要在这里找到导火线的原因，还有更多可怕的怪物武器来势汹汹，势不可挡，他们的势力实在强大，玩家可否有信心来战胜这场危机？注：此款游戏还属于试玩阶段，想要体验完整版的，奇境将为各位发烧友们第一时间更新，敬请期待哈！','./game/preImages/b/20160805170058283.jpg','第一人称射击',1,'HTC Vive','键鼠|手柄','http://resource.vr-store.cn/appfile/16a694d9d7464c1ebf25bc71ef3bc76a.zip',10,'1.09G',10.0,'591vr','<p>好消息，大好消息！HTC射手游戏重磅来袭，拥有VIVE的发烧友们再也不用担心没有好玩的VR游戏啦！在此款游戏中世界的终结时间即将来临，玩家务必需要在这里找到导火线的原因，还有更多可怕的怪物武器来势汹汹，势不可挡，他们的势力实在强大，玩家可否有信心来战胜这场危机？注：此款游戏还属于试玩阶段，想要体验完整版的，奇境将为各位发烧友们第一时间更新，敬请期待哈！</p><p><img src="./game/details/b/20160805170058284.png"></p><p><img src="./game/details/b/20160805170058285.png"></p><p><img src="./game/details/b/20160805170058286.png"></p>');
INSERT INTO `game`(title, create_time, company, preinfo, preimage, type, system, display_device, control_device, download_url, download_num, installation_size, score, source, content) VALUES ('全民棒球王','2016-02-22 00:00:00',350,'全民棒球王是一款前所未有的VR棒球投手游戏。这款游戏仅需轻叹手指，便可享受动感投球，简单而又充满乐趣。简单、容易而又充满乐趣！操作简单，游戏短小，可使玩家在任何地方、任何时间玩耍！','./game/preImages/b/20160805170058287.jpg','动作游戏',1,'Oculus DK2','手柄|键鼠','http://resource.vr-store.cn/appfile/d7f4722b86d5499ebe02bbd1ed07d328.zip',10,'164.33M',10.0,'591vr','<p>全民棒球王是一款前所未有的VR棒球投手游戏。这款游戏仅需轻叹手指，便可享受动感投球，简单而又充满乐趣。简单、容易而又充满乐趣！操作简单，游戏短小，可使玩家在任何地方、任何时间玩耍！</p><p><img src="./game/details/b/20160805170058288.jpg"></p>');
INSERT INTO `game`(title, create_time, company, preinfo, preimage, type, system, display_device, control_device, download_url, download_num, installation_size, score, source, content) VALUES ('飙车一族','2016-03-04 00:00:00',351,'《飙车一族（Racecraft）》是一款由Vae Victis Games制作的赛车游戏。游戏采用了沙盒模式，对于赛车游戏来说是非常新颖的体验。在游戏中你可以创造无数栩栩如生的赛道，以及改造你的赛车使它们在所有单人比赛和在线竞技中达到最佳的性能。新颖的玩法和创新的功能，《飙车一族》旨在打造赛车游戏领域真正新鲜的产品。注：抢先体验最近版本的试玩游戏，如有存在BUG，请在下面留言！','./game/preImages/b/20160805170058289.jpg','赛车竞速',1,'3Glasses|Oculus DK1|Oculus DK2|Deepoon|3Glasses','键鼠','http://resource.vr-store.cn/appfile/66231888fedd487ea90b64d064156f16.zip',10,'481.43M',10.0,'591vr','<p>《飙车一族（Racecraft）》是一款由Vae Victis Games制作的赛车游戏。游戏采用了沙盒模式，对于赛车游戏来说是非常新颖的体验。在游戏中你可以创造无数栩栩如生的赛道，以及改造你的赛车使它们在所有单人比赛和在线竞技中达到最佳的性能。新颖的玩法和创新的功能，《飙车一族》旨在打造赛车游戏领域真正新鲜的产品。注：抢先体验最近版本的试玩游戏，如有存在BUG，请在下面留言！</p><p><img src="./game/details/b/20160805170058290.png"></p>');
INSERT INTO `game`(title, create_time, company, preinfo, preimage, type, system, display_device, control_device, download_url, download_num, installation_size, score, source, content) VALUES ('第21空间','2016-03-02 00:00:00',352,'街机风格的体验游戏，玩家扮演一个飞行员角色，发挥高机动攻击力和空舰杀伤力，你的目标是能快速猛烈的躲避攻击。','./game/preImages/b/20160805170059291.jpg','第一人称射击',1,'3Glasses|Oculus DK1|Oculus DK2|Deepoon','手柄|键鼠','http://resource.vr-store.cn/appfile/83ad12fb6d704fe2aaad208c09edd465.zip',10,'70.2M',10.0,'591vr','<p>街机风格的体验游戏，玩家扮演一个飞行员角色，发挥高机动攻击力和空舰杀伤力，你的目标是能快速猛烈的躲避攻击。</p><p><img src="./game/details/b/20160805170059292.png"></p><p><img src="./game/details/b/20160805170059293.png"></p><p><img src="./game/details/b/20160805170059294.png"></p>');
