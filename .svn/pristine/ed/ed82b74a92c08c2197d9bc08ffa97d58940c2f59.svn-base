INSERT INTO `game`(title, create_time, company, preinfo, preimage, type, system, display_device, control_device, download_url, download_num, installation_size, score, source, content) VALUES ('战斗机器人','2015-11-12 00:00:00',360,'画风偏卡通，&ldquo;上帝的视角&rdquo;是这款游戏的模式（听起来很霸气吧）游戏背景是在外星球上，玩家操纵卡通版的机器人几倍邪恶的机器人！游戏玩法，把设备放进VR眼镜通过头部摆动闪躲敌人的自动炮塔，保护自己并着机会攻击敌人，游戏里面可以手机能源来提高你的武器破坏力量！','./game/preImages/c/20160806133645380.jpg','格斗游戏',2,'手机设备','触屏','http://apkdl.913vr.com/app/zhandoujiqiren_2.0.apk',10,'36.7MB',4.0,'913vr','<p>画风偏卡通，&ldquo;上帝的视角&rdquo;是这款游戏的模式（听起来很霸气吧）游戏背景是在外星球上，玩家操纵卡通版的机器人几倍邪恶的机器人！游戏玩法，把设备放进VR眼镜通过头部摆动闪躲敌人的自动炮塔，保护自己并着机会攻击敌人，游戏里面可以手机能源来提高你的武器破坏力量！</p><p><img src="./game/details/c/20160806133645381.png"/></p><p><img src="./game/details/c/20160806133645382.png"/></p><p><img src="./game/details/c/20160806133645383.png"/></p>');
INSERT INTO `game`(title, create_time, company, preinfo, preimage, type, system, display_device, control_device, download_url, download_num, installation_size, score, source, content) VALUES ('登山吉普车之旅','2015-11-12 00:00:00',360,'您是不是在找一个实时仿真驱动吉普车的虚拟现实游戏呢？你和你的朋友在旅途中可以欣赏自然之美的丘陵地带。因此，这款游戏给您提供了这个机会，享受大自然。你的朋友是一个专业的吉普车司机，他驾驶吉普车在山上。你所需要做的工作就是享受，体验山尖高速奔驰的疯狂。您将参观高山，清澈的湖泊，桥梁以及隧道！通过你的车，也可以感受到急转弯与落实擦肩而过的快感！','./game/preImages/c/20160806133646384.jpg','动作游戏',2,'手机设备','触屏','http://apkdl.913vr.com/app/dengshanjipuchezhilv_2.0.apk',10,'32MB',3.0,'913vr','<p>您是不是在找一个实时仿真驱动吉普车的虚拟现实游戏呢？你和你的朋友在旅途中可以欣赏自然之美的丘陵地带。因此，这款游戏给您提供了这个机会，享受大自然。你的朋友是一个专业的吉普车司机，他驾驶吉普车在山上。你所需要做的工作就是享受，体验山尖高速奔驰的疯狂。您将参观高山，清澈的湖泊，桥梁以及隧道！通过你的车，也可以感受到急转弯与落实擦肩而过的快感！</p><p><img src="./game/details/c/20160806133646385.png"/></p><p><img src="./game/details/c/20160806133646386.png"/></p><p><img src="./game/details/c/20160806133646387.png"/></p>');
INSERT INTO `game`(title, create_time, company, preinfo, preimage, type, system, display_device, control_device, download_url, download_num, installation_size, score, source, content) VALUES ('侏罗纪过山车VR','2015-11-12 00:00:00',360,'《侏罗纪世界》又掀起了一轮恐龙热潮，而在恐龙环绕之中体验过山车的乐趣，足以让肾上腺素爆表，马上体验一把数亿年前的过山车乐趣吧。','./game/preImages/c/20160806133747388.jpg','漫游体验',2,'手机设备','触屏','http://apkdl.913vr.com/app/zhuluojiguoshangche.apk',10,'34.2MB',3.0,'913vr','<p>《侏罗纪世界》又掀起了一轮恐龙热潮，而在恐龙环绕之中体验过山车的乐趣，足以让肾上腺素爆表，马上体验一把数亿年前的过山车乐趣吧。</p><p><img src="./game/details/c/20160806133747389.jpg"/></p><p><img src="./game/details/c/20160806133747390.jpg"/></p><p><img src="./game/details/c/20160806133747391.jpg"/></p>');
