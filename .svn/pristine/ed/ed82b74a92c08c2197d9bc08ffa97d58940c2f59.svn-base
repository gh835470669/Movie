INSERT INTO `game`(title, create_time, company, preinfo, preimage, type, system, display_device, control_device, download_url, download_num, installation_size, score, source, content) VALUES ('AR拼拼乐','2016-07-29 00:00:00',611,'《AR拼拼乐》是一款带有自动识别、纠错功能的益智拼图积木。通过智能设备安装&ldquo;AR拼拼乐&rdquo;应用程序之后，开启智能设备后置摄像头扫描拼图，每当拼图成功后，都可以通过AR技术让绘制的画面变成立体的3D动画，与孩子们展开即时互动，开启前所未有的拼图体验!','./game/preImages/c/2016080609200520.jpg','休闲益智',2,'手机设备','陀螺仪','http://apkdl.913vr.com/app/arpingpingle.apk',10,'306MB',4.0,'913vr','<p>《AR拼拼乐》是一款带有自动识别、纠错功能的益智拼图积木。通过智能设备安装&ldquo;AR拼拼乐&rdquo;应用程序之后，开启智能设备后置摄像头扫描拼图，每当拼图成功后，都可以通过AR技术让绘制的画面变成立体的3D动画，与孩子们展开即时互动，开启前所未有的拼图体验!</p><p><img src="./game/details/c/2016080609200521.jpg"/></p><p><img src="./game/details/c/2016080609200522.jpg"/></p><p><img src="./game/details/c/2016080609200523.jpg"/></p>');
