INSERT INTO `game`(title, create_time, company, preinfo, preimage, type, system, display_device, control_device, download_url, download_num, installation_size, score, source, content) VALUES ('格斗战机','2015-11-3 00:00:00',360,'体验第一次世界大战时的绅士空战，这款游戏需要配合上蓝牙手柄或者键盘使用，操作简单但极富飞行乐趣，一次激烈的空战，一次畅快的享受','./game/preImages/c/20160806134115476.jpg','格斗游戏',2,'手机设备','触屏','http://apkdl.913vr.com/app/gedouzhanji.apk',10,'26.5M',3.0,'913vr','<p>体验第一次世界大战时的绅士空战，这款游戏需要配合上蓝牙手柄或者键盘使用，操作简单但极富飞行乐趣，一次激烈的空战，一次畅快的享受</p><p><img src="./game/details/c/20160806134115477.jpg"/></p><p><img src="./game/details/c/20160806134115478.jpg"/></p><p><img src="./game/details/c/20160806134115479.jpg"/></p>');
INSERT INTO `game`(title, create_time, company, preinfo, preimage, type, system, display_device, control_device, download_url, download_num, installation_size, score, source, content) VALUES ('Solar System VR','2015-11-9 00:00:00',360,'《Solar System VR》是一款安卓平台虚拟现实休闲游戏,带玩家在虚拟现实中环游太阳系!你可以看到太阳系中的八大行星和其他一些小星体,也能看到各种各种漂浮的陨石,另外游戏的行走速度比较慢,眩晕感低,分成适合VR新手体验.','./game/preImages/c/20160806134155480.jpg','漫游体验',2,'手机设备','触屏','http://apkdl.913vr.com/app/Solar System VR_3.0.apk',10,'27.4MB',3.0,'913vr','<p>《Solar System VR》是一款安卓平台虚拟现实休闲游戏,带玩家在虚拟现实中环游太阳系!你可以看到太阳系中的八大行星和其他一些小星体,也能看到各种各种漂浮的陨石,另外游戏的行走速度比较慢,眩晕感低,分成适合VR新手体验.</p><p><img src="./game/details/c/20160806134155481.jpg"/></p><p><img src="./game/details/c/20160806134155482.jpg"/></p><p><img src="./game/details/c/20160806134155483.jpg"/></p>');
INSERT INTO `game`(title, create_time, company, preinfo, preimage, type, system, display_device, control_device, download_url, download_num, installation_size, score, source, content) VALUES ('沙滩自行车','2016-07-02 00:00:00',611,'《沙滩自行车》是一款模拟自行车在海边骑行的虚拟现实游戏，在风光旖旎、艳阳烈日的浪漫沙滩上骑行穿过丛林以及一些神秘的古建筑。','./game/preImages/c/20160806134218484.jpg','漫游体验',3,'手机设备','触屏','https://itunes.apple.com/cn/app/vr-fitness/id960611040?l=en&amp;mt=8',10,'178MB',4.0,'913vr','<p>《沙滩自行车》是一款模拟自行车在海边骑行的虚拟现实游戏，在风光旖旎、艳阳烈日的浪漫沙滩上骑行穿过丛林以及一些神秘的古建筑。</p><p><img src="./game/details/c/20160806134218485.jpg"/></p><p><img src="./game/details/c/20160806134218486.jpg"/></p><p><img src="./game/details/c/20160806134218487.jpg"/></p>');
