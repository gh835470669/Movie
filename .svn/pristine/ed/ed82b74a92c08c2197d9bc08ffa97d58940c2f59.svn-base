INSERT INTO `game`(title, create_time, company, preinfo, preimage, type, system, display_device, control_device, download_url, download_num, installation_size, score, source, content) VALUES ('疯狂的冒险船','2015-11-11 00:00:00',360,'驾驶自行车,汽车,卡车或巴士固然很有兴趣,但你有没有试过驾驶一艘船吗?在流动的水波固有的惯性,这些东西要复杂的多 ,但特别有趣.这里谈到划船和参观胡波的组合.在这个虚拟现实基础的模拟,您将参观湖上的船.在这次巡航中,你会看到自然之美,热气球\鹰,美丽的岛屿还有水中的鱼.','./game/preImages/c/20160806134031440.jpg','漫游体验',2,'手机设备','触屏','http://apkdl.913vr.com/app/fenkuanmaoxianchuan_1.0.apk',10,'29.4MB',3.0,'913vr','<p>驾驶自行车,汽车,卡车或巴士固然很有兴趣,但你有没有试过驾驶一艘船吗?在流动的水波固有的惯性,这些东西要复杂的多 ,但特别有趣.这里谈到划船和参观胡波的组合.在这个虚拟现实基础的模拟,您将参观湖上的船.在这次巡航中,你会看到自然之美,热气球\鹰,美丽的岛屿还有水中的鱼.</p><p><img src="./game/details/c/20160806134031441.png"/></p><p><img src="./game/details/c/20160806134031442.png"/></p><p><img src="./game/details/c/20160806134031443.png"/></p>');
INSERT INTO `game`(title, create_time, company, preinfo, preimage, type, system, display_device, control_device, download_url, download_num, installation_size, score, source, content) VALUES ('Sisters（安卓版）','2015-11-9 00:00:00',360,'《sisters》是一个虚拟现实VR鬼故事，当你进入鬼故事场景中，你会害怕吗？不管怕不怕都要小心，您在观察这个屋子的时候务必要谨慎，因为你在这间屋子里并不受欢迎，它正在监视着你，让你有种毛骨悚然的感觉，配合着音乐，让你有种身临其境的感觉！游戏玩法：游戏本身没有过多的操作，只需要开启游戏带上VR设备即可体验。','./game/preImages/c/20160806134051444.jpg','漫游体验',2,'手机设备','触屏','http://apkdl.913vr.com/app/Sisters_Android_5.0.apk',10,'37.4MB',4.0,'913vr','<p>《sisters》是一个虚拟现实VR鬼故事，当你进入鬼故事场景中，你会害怕吗？不管怕不怕都要小心，您在观察这个屋子的时候务必要谨慎，因为你在这间屋子里并不受欢迎，它正在监视着你，让你有种毛骨悚然的感觉，配合着音乐，让你有种身临其境的感觉！游戏玩法：游戏本身没有过多的操作，只需要开启游戏带上VR设备即可体验。</p><p><img src="./game/details/c/20160806134051445.jpg"/></p><p><img src="./game/details/c/20160806134051446.jpg"/></p><p><img src="./game/details/c/20160806134051447.jpg"/></p>');
