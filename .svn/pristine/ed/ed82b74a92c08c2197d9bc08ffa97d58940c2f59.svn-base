INSERT INTO `game`(title, create_time, company, preinfo, preimage, type, system, display_device, control_device, download_url, download_num, installation_size, score, source, content) VALUES ('吉普车游非洲','2016-07-02 00:00:00',611,'《吉普车游非洲》是一款虚拟旅游VR，驾驶狂野的大吉普在神秘的非洲草原和森林中冒险，可以看到到这片大陆里狮子、长颈鹿等各种野生动物。','./game/preImages/c/20160806134222500.jpg','漫游体验',3,'手机设备','体感','https://itunes.apple.com/cn/app/vr-safari/id1023203267?l=en&amp;mt=8',10,'144MB',4.0,'913vr','<p>《吉普车游非洲》是一款虚拟旅游VR，驾驶狂野的大吉普在神秘的非洲草原和森林中冒险，可以看到到这片大陆里狮子、长颈鹿等各种野生动物。</p><p><img src="./game/details/c/20160806134222501.jpg"/></p><p><img src="./game/details/c/20160806134222502.jpg"/></p><p><img src="./game/details/c/20160806134222503.jpg"/></p>');
