.z_home-news {
    overflow: hidden;

    margin-top: 20px;
    height: 952px;
}

.z_home-news-header {
    overflow: hidden;

    height: 45px;
    line-height: 45px;

    vertical-align: center;
}

.z_mark1 {
    margin-top: 13px;
    margin-right: 10px;
    width: 8px;
    height: 18px;

    background-color: #1e6fff;
}

.z_home-news1 {
    width: 790px;
}

.z_home-news1-list {
    display: list-item;
    overflow: hidden;

    padding-top: 14px;
    padding-bottom: 14px;
    height: 144px;
}

.z_home-news1-list img {
    width: 224px;
    height: 144px;
}

.z_home-news1-intro {
    width: 584px;
}

/*add by Nicholas 7/22/2016*/
/*最新快讯，重置多行省略的参数*/
.z_home-news1-intro .w_multi-ellipsis {
    margin-top: 4px;
    height: 80px;
}

.z_home-news1-intro .w_multi-ellipsis:before {
    height: 80px;
}

.z_home-news1-intro .w_multi-ellipsis:after {
    top: -24px;
}

.z_home-news1-intro .w_multi-ellipsis p {
    font-size: 14px;
    line-height: 20px;

    color: #999;
}

.z_type {
    margin-top: 10px;
    height: 20px;

    line-height: 20px;
    vertical-align: center;
}

div.z_type p {
    margin-left: 20px;
    border-radius: 5px;
    width: 25px;
    height: 20px;

    font-size: 12px;
    text-align: center;

    color: #fff;
}

.z_type-colorY {
    background-color: #f6b11e;
}

.z_type-colorG {
    background-color: #bababa;
}

