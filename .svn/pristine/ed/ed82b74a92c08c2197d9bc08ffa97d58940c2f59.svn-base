INSERT INTO `game`(title, create_time, company, preinfo, preimage, type, system, display_device, control_device, download_url, download_num, installation_size, score, source, content) VALUES ('怪兽之怒：圣丹斯的抉择','2015-06-25 00:00:00',428,'愤怒的怪兽将要摧毁这个城市，千百万人的命运如今掌握在被困黑暗地下处的两个科学家手里。和怪兽一起战斗，体验游戏中刺激的场景。','./game/preImages/b/201608051702351680.jpg','冒险解谜',1,'3Glasses|Oculus DK1|Oculus DK2|Deepoon','键鼠','http://resource.vr-store.cn/appfile/c65f0e0ba6694b4a8ee660b510a65751.zip',10,'19.52M',10.0,'591vr','<p>愤怒的怪兽将要摧毁这个城市，千百万人的命运如今掌握在被困黑暗地下处的两个科学家手里。和怪兽一起战斗，体验游戏中刺激的场景。</p><p><img src="./game/details/b/201608051702351681.jpg"></p><p><img src="./game/details/b/201608051702351682.png"></p><p><img src="./game/details/b/201608051702351683.jpg"></p>');
