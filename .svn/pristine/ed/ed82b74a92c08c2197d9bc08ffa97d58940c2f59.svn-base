INSERT INTO `game`(title, create_time, company, preinfo, preimage, type, system, display_device, control_device, download_url, download_num, installation_size, score, source, content) VALUES ('阿波罗11号','2015-05-04 00:00:00',611,'阿波罗11号（Apollo 11）是美国国家航空航天局（NASA）的阿波罗计划（Project Apollo）中的第五次载人任务，是人类第一次登月任务，三位执行此任务的宇航员分别为指令长阿姆斯特朗（Neil Armstrong）、指令舱驾驶员迈克尔·科林斯（Michael Collins）与登月舱驾驶员巴兹·奥尔德林（Buzz Aldrin）。1969年7月20日，阿姆斯特朗与奥尔德林成为了首次踏上月球的人类。','./game/preImages/b/201608051704292268.jpg','漫游体验',1,'3Glasses|Oculus DK1|Oculus DK2|Deepoon','','http://resource.vr-store.cn/appfile/ApolloDemoV1.0-Oculus DK2.zip',10,'408.72M',10.0,'591vr','<p>阿波罗11号（Apollo 11）是美国国家航空航天局（NASA）的阿波罗计划（Project Apollo）中的第五次载人任务，是人类第一次登月任务，三位执行此任务的宇航员分别为指令长阿姆斯特朗（Neil Armstrong）、指令舱驾驶员迈克尔·科林斯（Michael Collins）与登月舱驾驶员巴兹·奥尔德林（Buzz Aldrin）。1969年7月20日，阿姆斯特朗与奥尔德林成为了首次踏上月球的人类。</p><p><img src="./game/details/b/201608051704292269.jpg"></p><p><img src="./game/details/b/201608051704292270.jpg"></p><p><img src="./game/details/b/201608051704292271.jpg"></p>');
INSERT INTO `game`(title, create_time, company, preinfo, preimage, type, system, display_device, control_device, download_url, download_num, installation_size, score, source, content) VALUES ('大摆锤','2015-05-18 00:00:00',611,'经典游乐园《大摆锤》给人带来刺激的升降体验，视觉上的冲击和动作的摇摆完全模拟气氛，在绝对安全的状态下可以完全释放自己的压抑。','./game/preImages/b/201608051704292272.jpg','动作游戏',1,'Oculus DK2','','http://resource.vr-store.cn/appfile/CyberSpaceOculus DK2_MM.zip',10,'191.91M',8.0,'591vr','<p>经典游乐园《大摆锤》给人带来刺激的升降体验，视觉上的冲击和动作的摇摆完全模拟气氛，在绝对安全的状态下可以完全释放自己的压抑。</p><p><img src="./game/details/b/201608051704292273.jpg"></p><p><img src="./game/details/b/201608051704292274.jpg"></p><p><img src="./game/details/b/201608051704292275.jpg"></p>');
