INSERT INTO `game`(title, create_time, company, preinfo, preimage, type, system, display_device, control_device, download_url, download_num, installation_size, score, source, content) VALUES ('射击外星人','2016-05-26 00:00:00',611,'射击外星人(VR FPS in Space)是一款iOS射击游戏，拿起手里的武器射杀潮水般涌来的外星人，它们通过传送门传送过来的，找到它们的入口，最终摧毁传送门，来获得生机。喜欢射击游戏的玩家可以下载体验一番!','./game/preImages/c/2016080609204556.jpg','飞行射击',3,'手机设备','陀螺仪','https://itunes.apple.com/cn/app/vr-fps-in-space-for-google/id1116972022?mt=8',10,'66.3MB',4.0,'913vr','<p>射击外星人(VR FPS in Space)是一款iOS射击游戏，拿起手里的武器射杀潮水般涌来的外星人，它们通过传送门传送过来的，找到它们的入口，最终摧毁传送门，来获得生机。喜欢射击游戏的玩家可以下载体验一番!</p><p><img src="./game/details/c/2016080609204557.jpg"/></p><p><img src="./game/details/c/2016080609204558.jpg"/></p><p><img src="./game/details/c/2016080609204559.jpg"/></p>');
