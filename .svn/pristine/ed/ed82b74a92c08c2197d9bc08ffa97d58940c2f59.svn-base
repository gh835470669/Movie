INSERT INTO `game`(title, create_time, company, preinfo, preimage, type, system, display_device, control_device, download_url, download_num, installation_size, score, source, content) VALUES ('Spelunky','2013-08-08 00:00:00',611,'Spelunky is a unique platformer with randomized levels that offer a challenging new experience each time you play. Journey deep underground and explore fantastic places filled with all manner of monsters, traps, and treasure. You\'ll have complete freedom while you navigate the fully-destructible environments and master their many secrets.','./game/preImages/d/20160807002835783.jpg','其他',1,'','','http://store.steampowered.com/app/239350/?snr=1_7_7_230_150_16',99,'',7.5,'steam','<p>Spelunky is a unique platformer with randomized levels that offer a challenging new experience each time you play. Journey deep underground and explore fantastic places filled with all manner of monsters, traps, and treasure. You\'ll have complete freedom while you navigate the fully-destructible environments and master their many secrets.</p><p><img src="./game/details/d/20160807002835784.jpg"></p><p><img src="./game/details/d/20160807002835785.jpg"></p><p><img src="./game/details/d/20160807002835786.jpg"></p>');
