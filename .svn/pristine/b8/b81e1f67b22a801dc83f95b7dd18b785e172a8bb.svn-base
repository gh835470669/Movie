package com.luwei.hackvr.pojo;

import java.io.Serializable;

public class DisplayDeviceMap implements Serializable {
    private Integer displayDeviceMapperId;

    private Integer displayDeviceId;

    private Integer gameId;

    private Byte flag;

    private static final long serialVersionUID = 1L;

    public Integer getDisplayDeviceMapperId() {
        return displayDeviceMapperId;
    }

    public void setDisplayDeviceMapperId(Integer displayDeviceMapperId) {
        this.displayDeviceMapperId = displayDeviceMapperId;
    }

    public Integer getDisplayDeviceId() {
        return displayDeviceId;
    }

    public void setDisplayDeviceId(Integer displayDeviceId) {
        this.displayDeviceId = displayDeviceId;
    }

    public Integer getGameId() {
        return gameId;
    }

    public void setGameId(Integer gameId) {
        this.gameId = gameId;
    }

    public Byte getFlag() {
        return flag;
    }

    public void setFlag(Byte flag) {
        this.flag = flag;
    }
}