INSERT INTO `game`(title, create_time, company, preinfo, preimage, type, system, display_device, control_device, download_url, download_num, installation_size, score, source, content) VALUES ('飞船入侵','2015-11-12 00:00:00',360,'在宇宙中驾驶太空旅行船，你正在遭受意外，被迫降落在一个未知的星球上......为了生存你得开始冒险。这款游戏需要蓝牙手柄配合，体验感十足！游戏的气氛也很吓人，不时还会出现一些鬼影！在太空飞船里，玩家需要找到5个红色细胞，这5个细胞发布在飞船四周可能在地上或者其他某个角落里，玩家需要仔细的寻找红色的光电哦！','./game/preImages/c/20160806133200312.jpg','漫游体验',2,'手机设备','触屏','http://apkdl.913vr.com/app/feichuanruqin7.0.apk',10,'40.6MB',3.0,'913vr','<p>在宇宙中驾驶太空旅行船，你正在遭受意外，被迫降落在一个未知的星球上......为了生存你得开始冒险。这款游戏需要蓝牙手柄配合，体验感十足！游戏的气氛也很吓人，不时还会出现一些鬼影！在太空飞船里，玩家需要找到5个红色细胞，这5个细胞发布在飞船四周可能在地上或者其他某个角落里，玩家需要仔细的寻找红色的光电哦！</p><p><img src="./game/details/c/20160806133200313.png"/></p><p><img src="./game/details/c/20160806133200314.png"/></p><p><img src="./game/details/c/20160806133200315.png"/></p>');
