  `download_url` varchar(500) DEFAULT NULL COMMENT '下载链接',
  `download_num` int(11) DEFAULT '0' COMMENT '下载数目',
  `installation_size` varchar(50) DEFAULT NULL COMMENT '安装包大小',
  `score` decimal(3,2) DEFAULT '5.00' COMMENT '评分',
  `author` varchar(50) DEFAULT NULL COMMENT '编辑作者',
