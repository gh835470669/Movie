INSERT INTO `game`(title, create_time, company, preinfo, preimage, type, system, display_device, control_device, download_url, download_num, installation_size, score, source, content) VALUES ('樱花绽放','2015-11-11 00:00:00',360,'每年的3月15-4月15都是全世界樱花盛开的季节，全世界有大量种植樱花树的城市都有举办樱花节。也许玩家们很想去现场观摩感受一下，但是由于各种原因无法到现场的话也不用担心！这款游戏真实的还原樱花盛开时的场景！玩家可以放松心态，跟随轻快的音乐尽情的欣赏樱花打开的乐趣！','./game/preImages/c/20160806133422348.jpg','漫游体验',2,'手机设备','触屏','http://apkdl.913vr.com/app/yinhuazhangfang_3.0.apk',10,'29.0MB',3.0,'913vr','<p>每年的3月15-4月15都是全世界樱花盛开的季节，全世界有大量种植樱花树的城市都有举办樱花节。也许玩家们很想去现场观摩感受一下，但是由于各种原因无法到现场的话也不用担心！这款游戏真实的还原樱花盛开时的场景！玩家可以放松心态，跟随轻快的音乐尽情的欣赏樱花打开的乐趣！</p><p><img src="./game/details/c/20160806133422349.png"/></p><p><img src="./game/details/c/20160806133422350.png"/></p><p><img src="./game/details/c/20160806133422351.png"/></p>');
