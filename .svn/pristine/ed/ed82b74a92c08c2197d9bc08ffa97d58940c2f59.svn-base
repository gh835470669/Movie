INSERT INTO `game`(title, create_time, company, preinfo, preimage, type, system, display_device, control_device, download_url, download_num, installation_size, score, source, content) VALUES ('Alien Apartment VR','2015-11-10 00:00:00',360,'安卓适配虚拟现实休闲场景一枚！外星球上有着超大天窗的公寓、你和娇妻抬头仰望就能看到各种美丽星球和璀璨银河--这就是《Alien Apartment VR》想让你体验的！你可以在这个非常美的公寓中走走，和它进行互动；开关抽屉和灯，或躺在熟睡妻子的身边对着壮丽的太空景色发呆，又或和你的朋友进行对话。','./game/preImages/c/20160806133848400.jpg','漫游体验',2,'手机设备','触屏','http://apkdl.913vr.com/app/Alien Apartment VR_9.0.apk',10,'30.4MB',3.0,'913vr','<p>安卓适配虚拟现实休闲场景一枚！外星球上有着超大天窗的公寓、你和娇妻抬头仰望就能看到各种美丽星球和璀璨银河--这就是《Alien Apartment VR》想让你体验的！你可以在这个非常美的公寓中走走，和它进行互动；开关抽屉和灯，或躺在熟睡妻子的身边对着壮丽的太空景色发呆，又或和你的朋友进行对话。</p><p><img src="./game/details/c/20160806133848401.jpg"/></p><p><img src="./game/details/c/20160806133848402.jpg"/></p><p><img src="./game/details/c/20160806133848403.jpg"/></p>');
