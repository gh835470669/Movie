INSERT INTO `game`(title, create_time, company, preinfo, preimage, type, system, display_device, control_device, download_url, download_num, installation_size, score, source, content) VALUES ('Private Eye','2015-12-17 00:00:00',382,'脚伤让你被困在自己的公寓无所事事，却在偶然发现了对街公寓一桩凶杀案的蛛丝马迹，黑手党、偷情的情侣、淘气男孩、谋杀&mdash;&mdash;你开始&ldquo;私家侦探生涯&rdquo;，用望远镜观察进出公寓人群的行迹。你拨开迷雾寻找真凶，却不知道自己也正走向危险&hellip;&hellip;','./game/preImages/c/20160806132407152.jpg','休闲益智',1,'Oculus DK2|Oculus DK1|3Glasses|DeePoon','键鼠','http://pcdl.913vr.com/pc/Private Eye.zip',10,'330MB',4.0,'913vr','<p>脚伤让你被困在自己的公寓无所事事，却在偶然发现了对街公寓一桩凶杀案的蛛丝马迹，黑手党、偷情的情侣、淘气男孩、谋杀&mdash;&mdash;你开始&ldquo;私家侦探生涯&rdquo;，用望远镜观察进出公寓人群的行迹。你拨开迷雾寻找真凶，却不知道自己也正走向危险&hellip;&hellip;</p><p><img src="./game/details/c/20160806132407153.jpg"/></p><p><img src="./game/details/c/20160806132407154.jpg"/></p><p><img src="./game/details/c/20160806132407155.jpg"/></p>');
