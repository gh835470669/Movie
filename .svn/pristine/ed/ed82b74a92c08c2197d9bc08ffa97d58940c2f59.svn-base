INSERT INTO `game`(title, create_time, company, preinfo, preimage, type, system, display_device, control_device, download_url, download_num, installation_size, score, source, content) VALUES ('Mind Unleashed','2016-05-05 00:00:00',105,'15元的价格实际上是相当便宜的。《Mind Unleashed》是一款动作游戏，玩家还需要与对手战斗。不需要手势控制器，戴上头盔，拿起 &nbsp;XBOX 手柄，马上就能战斗。','./game/preImages/a/20160805110013430.jpg','动作游戏',1,'HTC Vive|Oculus CV1','XboxOne手柄','http://www.moduovr.com/front/app/download?id=274',25,'',7.6,'moduo魔多','<p>15元的价格实际上是相当便宜的。</p><p><img src="./game/details/a/20160805110013431.jpg"></img></p><p>《Mind Unleashed》是一款动作游戏，玩家还需要与对手战斗。不需要手势控制器，戴上头盔，拿起  XBOX 手柄，马上就能战斗。</p>');
INSERT INTO `game`(title, create_time, company, preinfo, preimage, type, system, display_device, control_device, download_url, download_num, installation_size, score, source, content) VALUES ('Egypt','2016-05-19 00:00:00',0,'步行在埃及神秘的房间，埃及金字塔、埃及法老不朽传说，这些都将呈现在你眼前。漫步在这封锁千年神秘而恐惧的世界，你没有地图、也没有同伴，你必须找到逃离出路。','./game/preImages/a/20160805110013432.png','休闲益智',2,'Cardboard','','http://www.moduovr.com/front/app/download?id=275',36,'48M',6.5,'moduo魔多','<p><img src="./game/details/a/20160805110013433.png"></img></p><p>步行在埃及神秘的房间，埃及金字塔、埃及法老不朽传说，这些都将呈现在你眼前。漫步在这封锁千年神秘而恐惧的世界，你没有地图、也没有同伴，你必须找到逃离出路。</p>');
