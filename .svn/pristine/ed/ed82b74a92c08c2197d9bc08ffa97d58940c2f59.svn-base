INSERT INTO `game`(title, create_time, company, preinfo, preimage, type, system, display_device, control_device, download_url, download_num, installation_size, score, source, content) VALUES ('沙漠过山车','2016-07-08 00:00:00',293,'游戏是在沙漠中进行过山车冒险，画面内容细致，音效也很出色，真实度很高，但体验时间较短，喜欢的人可以下来尝试。','./game/preImages/b/2016080517004244.jpg','冒险解谜',1,'Oculus DK2|Deepoon','键鼠','http://resource.vr-store.cn/appfile/b81aa2e34f804536abe4aa608b00db5a.zip',10,'703.74M',10.0,'591vr','<p>游戏是在沙漠中进行过山车冒险，画面内容细致，音效也很出色，真实度很高，但体验时间较短，喜欢的人可以下来尝试。</p><p><img src="./game/details/b/2016080517004245.jpg"></p><p><img src="./game/details/b/2016080517004246.jpg"></p><p><img src="./game/details/b/2016080517004247.jpg"></p>');
INSERT INTO `game`(title, create_time, company, preinfo, preimage, type, system, display_device, control_device, download_url, download_num, installation_size, score, source, content) VALUES ('Rob the bot','2016-07-05 00:00:00',199,'这是一款小游戏，我们选择卡通的形象是希望让你能享受VR的乐趣，能够让你关注于虚拟现实的发展。','./game/preImages/b/2016080517004348.jpg','休闲益智',1,'Oculus DK2|Deepoon','键鼠','http://resource.vr-store.cn/appfile/cfc42b3c5ad24c40bf0d975ac813d849.zip',10,'51.11M',10.0,'591vr','<p>这是一款小游戏，我们选择卡通的形象是希望让你能享受VR的乐趣，能够让你关注于虚拟现实的发展。</p><p><img src="./game/details/b/2016080517004349.jpg"></p><p><img src="./game/details/b/2016080517004350.jpg"></p><p><img src="./game/details/b/2016080517004351.jpg"></p>');
INSERT INTO `game`(title, create_time, company, preinfo, preimage, type, system, display_device, control_device, download_url, download_num, installation_size, score, source, content) VALUES ('开心屋','2016-07-15 00:00:00',294,'英伟达以开发高性能游戏显卡而闻名，但却没有出版过自己的游戏，直到现在。该公司推出了商场风格、迷你沉浸式游戏VR Funhouse，用于展示虚拟现实的潜力，并宣传英伟达的技术如何提升体验。','./game/preImages/b/2016080517004352.jpg','休闲益智',1,'HTC Vive','手柄|键鼠','http://resource.vr-store.cn/appfile/f52374a2c5fb4f509fc8eb2043d069db.zip',10,'1.06G',10.0,'591vr','<p>英伟达以开发高性能游戏显卡而闻名，但却没有出版过自己的游戏，直到现在。该公司推出了商场风格、迷你沉浸式游戏VR Funhouse，用于展示虚拟现实的潜力，并宣传英伟达的技术如何提升体验。</p><p><img src="./game/details/b/2016080517004353.jpg"></p><p><img src="./game/details/b/2016080517004354.jpg"></p><p><img src="./game/details/b/2016080517004355.jpg"></p>');
INSERT INTO `game`(title, create_time, company, preinfo, preimage, type, system, display_device, control_device, download_url, download_num, installation_size, score, source, content) VALUES ('Helix过山车','2016-06-27 00:00:00',611,'《Helix过山车VR》是最早一批虚拟现实过山车游戏，与《城堡过山车》、《UE4室内过山车》形成了三足鼎力局面，是当时过山车大军中最耀眼的三颗星。','./game/preImages/b/2016080517004356.jpg','冒险解谜',1,'Oculus DK1|Oculus DK2|Deepoon','键鼠','http://resource.vr-store.cn/appfile/c325037152c542deb633d0a6972c7807.zip',10,'199.64M',10.0,'591vr','<p>《Helix过山车VR》是最早一批虚拟现实过山车游戏，与《城堡过山车》、《UE4室内过山车》形成了三足鼎力局面，是当时过山车大军中最耀眼的三颗星。</p><p><img src="./game/details/b/2016080517004357.jpg"></p><p><img src="./game/details/b/2016080517004358.jpg"></p><p><img src="./game/details/b/2016080517004359.jpg"></p>');
