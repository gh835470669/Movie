.w_article h1 {
    font-size: 32px;

    color: #121212;
}

.w_article p {
    margin-top: 20px;
    margin-bottom: 20px;

    font-size: 14px;

    color: #333;
}

.w_article p img {
    display: block;

    width: 746px;
    height: auto;
}

.w_relevant {
    margin-bottom: 74px;
}

.w_relevant .w_h3 {
    margin-bottom: 0px;
    padding-left: 6px;
}

.w_relevant-ul li {

    border-bottom: 1px solid #ccc;
}

.w_relevant-ul li a {
    display: block;
    
    padding-left: 18px;
    width: 262px;
    height: 46px;

    font-size: 14px;
    line-height: 46px;

    color: #121212;
    background-image: url("../images/square.png");
    background-repeat: no-repeat;
    background-position: 8px 22px;
}
