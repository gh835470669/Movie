INSERT INTO `game`(title, create_time, company, preinfo, preimage, type, system, display_device, control_device, download_url, download_num, installation_size, score, source, content) VALUES ('金币大作战','2015-11-13 00:00:00',360,'游戏设定在峡谷和熔岩之间，你需要收集尽可能多的金币，收集越多得分越高，操作很简单，头部控制方向，蓝牙手柄控制跳跃。','./game/preImages/c/20160806133443360.jpg','动作游戏',2,'手机设备','触屏','http://apkdl.913vr.com/app/jinbidazuozhan.apk',10,'11.2MB',4.0,'913vr','<p>游戏设定在峡谷和熔岩之间，你需要收集尽可能多的金币，收集越多得分越高，操作很简单，头部控制方向，蓝牙手柄控制跳跃。</p><p><img src="./game/details/c/20160806133443361.jpg"/></p><p><img src="./game/details/c/20160806133443362.jpg"/></p><p><img src="./game/details/c/20160806133443363.jpg"/></p>');
INSERT INTO `game`(title, create_time, company, preinfo, preimage, type, system, display_device, control_device, download_url, download_num, installation_size, score, source, content) VALUES ('森林动物园','2015-11-11 00:00:00',360,'这款游戏可以给那些想看野生动物却没有机会的朋友们带来不少惊喜哦！这种模拟有一个伟大的教育价值，特别是孩子们。在这个模拟，您将参观狮子、狼、熊、野猪、兔子、狐狸、鹿、大象、斑马、乌鸦、老鹰、鸽子、麻雀等。','./game/preImages/c/20160806133444364.jpg','漫游体验',2,'手机设备','触屏','http://apkdl.913vr.com/app/senlindongwuyuan_3.0.apk',10,'42.5MB',4.0,'913vr','<p>这款游戏可以给那些想看野生动物却没有机会的朋友们带来不少惊喜哦！这种模拟有一个伟大的教育价值，特别是孩子们。在这个模拟，您将参观狮子、狼、熊、野猪、兔子、狐狸、鹿、大象、斑马、乌鸦、老鹰、鸽子、麻雀等。</p><p><img src="./game/details/c/20160806133444365.png"/></p><p><img src="./game/details/c/20160806133444366.png"/></p><p><img src="./game/details/c/20160806133444367.png"/></p>');
INSERT INTO `game`(title, create_time, company, preinfo, preimage, type, system, display_device, control_device, download_url, download_num, installation_size, score, source, content) VALUES ('异性大作战','2015-11-12 00:00:00',360,'这是一款简单而又容易让人上瘾的虚拟现实第一人称射击游戏，玩家停留在虚拟现实世界里，并且不能走动的。来势汹汹的异性会慢慢的来到玩家身边，而玩家必须通过您最值得信赖的HK416半自动步枪来保护自己。如果异性靠的太近，游戏结束。你生存的时间越长，更对的异性将会出现。','./game/preImages/c/20160806133503368.jpg','格斗游戏',2,'手机设备','触屏','http://apkdl.913vr.com/app/yixingdazuozhan_2.0.apk',10,'36.3MB',3.0,'913vr','<p>这是一款简单而又容易让人上瘾的虚拟现实第一人称射击游戏，玩家停留在虚拟现实世界里，并且不能走动的。来势汹汹的异性会慢慢的来到玩家身边，而玩家必须通过您最值得信赖的HK416半自动步枪来保护自己。如果异性靠的太近，游戏结束。你生存的时间越长，更对的异性将会出现。</p><p><img src="./game/details/c/20160806133503369.png"/></p><p><img src="./game/details/c/20160806133503370.png"/></p><p><img src="./game/details/c/20160806133503371.png"/></p>');
INSERT INTO `game`(title, create_time, company, preinfo, preimage, type, system, display_device, control_device, download_url, download_num, installation_size, score, source, content) VALUES ('恐龙的土地VR','2015-11-13 00:00:00',360,'这是《侏罗纪公园》的虚拟现实版，霸王龙、剑龙、翼龙等一系列恐龙从你的身边路过，近距离观察恐龙，感受恐龙的魅力吧！','./game/preImages/c/20160806133544372.jpg','漫游体验',2,'手机设备','触屏','http://apkdl.913vr.com/app/konglongdetudiVR_3.0.apk',10,'45.9MB',3.0,'913vr','<p>这是《侏罗纪公园》的虚拟现实版，霸王龙、剑龙、翼龙等一系列恐龙从你的身边路过，近距离观察恐龙，感受恐龙的魅力吧！</p><p><img src="./game/details/c/20160806133544373.jpg"/></p><p><img src="./game/details/c/20160806133544374.jpg"/></p><p><img src="./game/details/c/20160806133544375.jpg"/></p>');
