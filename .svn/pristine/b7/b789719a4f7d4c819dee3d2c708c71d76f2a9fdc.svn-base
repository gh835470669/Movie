    height: 100%;
}

.z_backstage-publish {
    position: absolute;
    right: 30px;

    margin-top: 15px;
    width: 100px;
    height: 30px;

    font-size: 15px;
}

.z_backstage-publish:hover {
    background-color: #dcdcdc;

    cursor: pointer;
}

.z_backstage-back {
    position: absolute;
    right: 140px;

    margin-top: 15px;
    width: 100px;
    height: 30px;

    font-size: 15px;
}

.z_backstage-back:hover {
    background-color: #dcdcdc;

    cursor: pointer;
}

