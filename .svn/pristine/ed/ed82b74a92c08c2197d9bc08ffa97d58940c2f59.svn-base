INSERT INTO `game`(title, create_time, company, preinfo, preimage, type, system, display_device, control_device, download_url, download_num, installation_size, score, source, content) VALUES ('小行星VR','2016-07-20 00:00:00',611,'小行星VR(Asteroids VR)是一款手机端飞机类射击游戏。玩家在游戏中将会操控一艘宇宙飞船在行星中穿梭，沿途会遇到各种危险，各种的障碍物阻挡玩家前行，同时有不明的陨石会撞向玩家飞船，玩家需要攻击陨石才能通关','./game/preImages/c/2016080609220892.jpg','飞行射击',2,'手机设备','陀螺仪','http://apkdl.913vr.com/app/asteroidsvr.apk',10,'40.27M',3.0,'913vr','<p>小行星VR(Asteroids VR)是一款手机端飞机类射击游戏。玩家在游戏中将会操控一艘宇宙飞船在行星中穿梭，沿途会遇到各种危险，各种的障碍物阻挡玩家前行，同时有不明的陨石会撞向玩家飞船，玩家需要攻击陨石才能通关</p><p><img src="./game/details/c/2016080609220893.jpg"/></p><p><img src="./game/details/c/2016080609220894.jpg"/></p><p><img src="./game/details/c/2016080609220895.jpg"/></p>');
