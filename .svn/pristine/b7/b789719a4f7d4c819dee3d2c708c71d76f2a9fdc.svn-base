.z_backstage-fix {
    margin-right: auto;
    margin-left: auto;
    width: 930px;
}

.z_container {
    height: 100%;
}

body {
    font-family: "microsoft yahei";
}

.z_backstage {
    width: 100%;
    height: 100%;
    background-color: #f0f0f0;
}

.z_backstage-body {
    margin-right: auto;
    margin-left: auto;
    width: 930px;
    height: 82%;
}

.z_backstage-header {
    margin-bottom: 1%;
    height: 70px;

    line-height: 70px;

    background-color: #dcdcdc;
}

.z_backstage-menu {
    position: relative;
    height: 100%;
}

.z_backstage-menu-list {
    overflow: hidden;

    width: 100px;
    height: 70px;

    line-height: 70px;
    text-align: center;

    color: #707070;
}

.z_backstage-menu-list:hover {
    color: #fff;
    background-color: #707070;

    cursor: pointer;
}

.z_backstage-menu-son {
    position: absolute;

    width: 100px;
}

.z_backstage-menu-son-list {
    height: 70px;

    line-height: 70px;
    text-align: center;

    color: #707070;
    background-color: #dcdcdc;
}

.z_backstage-menu-son-list:hover {
    color: #fff;
    background-color: #707070;

    cursor: pointer;
}

.z_mt {
    margin-top: 100px;
}

.z_backstage-menu-first {
    color: #fff;
    background-color: #707070;
}

.z_backstage-content {
    overflow-x: hidden;
    overflow-y: scroll;

    margin-right: auto;
    margin-left: auto;
    width: 930px;
    height: 98%;

}

.z_backstage-content-title {
    overflow: hidden;

    width: 30%;

    color: #707070;
}

