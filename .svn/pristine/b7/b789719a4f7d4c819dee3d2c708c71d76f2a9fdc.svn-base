    height: 55px;

    font-size: 12px;
}

.z_backstage-type {
    height: 15px;

    line-height: 15px;
}

.z_backstage-type p {
    border-radius: 5px;
    width: 30px;
    
    font-size: 12px;
    text-align: center;
    
    color: #fff;
    background-color: #f6b01e;
}

.z_backstage-type span {

    margin-left: 10px;
    font-size: 12px;
}

.z_backstage-checkbox {
    width: 40px;
    height: 100px;

    line-height: 100px;
}

.z_checkbox {
    /*display: none;*/

    width: 20px;
    height: 20px;
}

.z_checkbox:hover {
    cursor: pointer;
}

.z_backstage-list-flag {
    font-size: 10px;

    color: red;
}

.z_hide-content {
