INSERT INTO `game`(title, create_time, company, preinfo, preimage, type, system, display_device, control_device, download_url, download_num, installation_size, score, source, content) VALUES ('超能滑板','2016-07-05 00:00:00',296,'游戏中的简单画面让玩家有一种舒心的感觉，工作累了不妨暂时忘掉城市中的喧嚣，跟着小动物们一起穿梭在森林里面，在森林中躲开各种石头与树木，在跳台上做出高难度的特技，让我们一起出发吧！','./game/preImages/b/2016080517004364.jpg','休闲益智',1,'Oculus DK2|Deepoon','键鼠','http://resource.vr-store.cn/appfile/6b1a0532619f42d08db35218161fd70c.zip',10,'29.57M',10.0,'591vr','<p>游戏中的简单画面让玩家有一种舒心的感觉，工作累了不妨暂时忘掉城市中的喧嚣，跟着小动物们一起穿梭在森林里面，在森林中躲开各种石头与树木，在跳台上做出高难度的特技，让我们一起出发吧！</p><p><img src="./game/details/b/2016080517004365.jpg"></p><p><img src="./game/details/b/2016080517004366.jpg"></p><p><img src="./game/details/b/2016080517004367.jpg"></p>');
