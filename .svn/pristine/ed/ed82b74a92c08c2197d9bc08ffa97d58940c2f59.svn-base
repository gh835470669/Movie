INSERT INTO `game`(title, create_time, company, preinfo, preimage, type, system, display_device, control_device, download_url, download_num, installation_size, score, source, content) VALUES ('户外猎鸭','2016-07-07 00:00:00',611,'户外猎鸭的操作比较简单，只需要通过手机，将画面正中的瞄准镜对准天下的野鸭，系统就会自动射击。','./game/preImages/c/2016080609200528.jpg','飞行射击',2,'手机设备','陀螺仪','http://apkdl.913vr.com/app/dieetagen.apk',10,'12.58MB',4.0,'913vr','<p>户外猎鸭的操作比较简单，只需要通过手机，将画面正中的瞄准镜对准天下的野鸭，系统就会自动射击。</p><p><img src="./game/details/c/2016080609200529.jpg"/></p><p><img src="./game/details/c/2016080609200530.jpg"/></p><p><img src="./game/details/c/2016080609200531.jpg"/></p>');
INSERT INTO `game`(title, create_time, company, preinfo, preimage, type, system, display_device, control_device, download_url, download_num, installation_size, score, source, content) VALUES ('幻实英雄','2016-08-03 00:00:00',611,'首款AR对战游戏&mdash;&mdash;幻实英雄，正式上线!打造&ldquo;卡游&rdquo;新模式!游戏基于AR增强现实技术，采用实时实景的全新对战形式，以震撼的视觉，带给您绝无仅有的操作体验!','./game/preImages/c/2016080609202132.jpg','角色扮演',2,'手机设备','陀螺仪','http://apkdl.913vr.com/app/huanshiyingxiong.apk',10,'52.5MB',4.0,'913vr','<p>首款AR对战游戏&mdash;&mdash;幻实英雄，正式上线!打造&ldquo;卡游&rdquo;新模式!游戏基于AR增强现实技术，采用实时实景的全新对战形式，以震撼的视觉，带给您绝无仅有的操作体验!</p><p><img src="./game/details/c/2016080609202133.jpg"/></p><p><img src="./game/details/c/2016080609202134.jpg"/></p><p><img src="./game/details/c/2016080609202135.jpg"/></p>');
INSERT INTO `game`(title, create_time, company, preinfo, preimage, type, system, display_device, control_device, download_url, download_num, installation_size, score, source, content) VALUES ('拯救水熊虫','2016-02-02 00:00:00',146,'拯救水熊虫VR是一款虚拟现实休闲冒险游戏，在拯救水熊虫VR Water Bears VR的世界中通过接水管来将水送个饥渴的水熊虫。随着关卡的推进，游戏地形会不断地变得复杂，是款非常有趣的游戏。','./game/preImages/c/2016080609202136.jpg','休闲益智',1,'HTC VIVE','键鼠','http://store.steampowered.com/app/394130/',10,'500MB',4.0,'913vr','<p>拯救水熊虫VR是一款虚拟现实休闲冒险游戏，在拯救水熊虫VR Water Bears VR的世界中通过接水管来将水送个饥渴的水熊虫。随着关卡的推进，游戏地形会不断地变得复杂，是款非常有趣的游戏。</p><p><img src="./game/details/c/2016080609202137.jpg"/></p><p><img src="./game/details/c/2016080609202138.jpg"/></p><p><img src="./game/details/c/2016080609202139.jpg"/></p>');
