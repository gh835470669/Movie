INSERT INTO `game`(title, create_time, company, preinfo, preimage, type, system, display_device, control_device, download_url, download_num, installation_size, score, source, content) VALUES ('奔腾小马','2014-10-10 00:00:00',611,'奔腾小马是安卓适配的休闲类虚拟现实游戏，在这个demo里，你变成了一只欢腾的小马匹，和其他的小马们一起肆意驰骋在蓝天绿树间的大草原上。','./game/preImages/c/201608061048593.jpg','漫游体验',2,'手机设备','陀螺仪','http://apkdl.913vr.com/app/JanRaacke.apk',10,'18.4M',5.0,'913vr','<p>奔腾小马是安卓适配的休闲类虚拟现实游戏，在这个demo里，你变成了一只欢腾的小马匹，和其他的小马们一起肆意驰骋在蓝天绿树间的大草原上。</p><p><img src="./game/details/c/201608061048594.jpg"/></p><p><img src="./game/details/c/201608061048595.jpg"/></p><p><img src="./game/details/c/201608061048596.jpg"/></p>');
