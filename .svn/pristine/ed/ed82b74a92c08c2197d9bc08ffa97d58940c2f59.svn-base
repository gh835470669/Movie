INSERT INTO `game`(title, create_time, company, preinfo, preimage, type, system, display_device, control_device, download_url, download_num, installation_size, score, source, content) VALUES ('追梦客之阆中','2015-10-29 00:00:00',360,'追梦客系列作品，中国四大保存最完整的古镇之一的阆中古城，位于阆中新城的旁边、嘉陵江畔。古城阆中的建筑风格体现了我国古代的居住风水观，棋盘式的古城格局，融南北风格于一体的建筑群。','./game/preImages/c/20160806135057624.jpg','漫游体验',2,'手机设备','触屏','http://apkdl.913vr.com/app/zmklangzhong.apk',10,'53.9M',3.0,'913vr','<p>追梦客系列作品，中国四大保存最完整的古镇之一的阆中古城，位于阆中新城的旁边、嘉陵江畔。古城阆中的建筑风格体现了我国古代的居住风水观，棋盘式的古城格局，融南北风格于一体的建筑群。</p><p><img src="./game/details/c/20160806135057625.jpg"/></p><p><img src="./game/details/c/20160806135057626.jpg"/></p><p><img src="./game/details/c/20160806135057627.jpg"/></p>');
